  <xs:element name="etcsRoute" type="signalling:EtcsRoute"/>
  <xs:complexType name="EtcsRoute">
    <xs:annotation>
      <xs:documentation>A route specialised for ETCS purposes.

</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Route">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="disallowsStopIfInSrBaliseGroup" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Refers to a balise group in the route where a stop-if-in-SR is not allowed. 
See SRS-026-7 packet 137, Stop if in Staff Responsible.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsBaliseGroup</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="excludesBaliseGroup" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>List of balises that are excluded from the link list of a MA. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsBaliseGroup</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasMaSection" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The route has a number of MA sections used for triggering the section timer.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsMaSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasSrBalise" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>List of balise groups that can accompany a SR-MA.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsBaliseGroup</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTempShuntingArea" type="signalling:StandstillDetectionForShunting">
            <xs:annotation>
              <xs:documentation>A timer per temporary shunting area.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="shuntingModeRoute" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>indicates that at the end of the route, the train receives a shunting mode profile.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsLevelCrossingSystem" type="signalling:EtcsLevelCrossingSystem"/>
  <xs:complexType name="EtcsLevelCrossingSystem">
    <xs:annotation>
      <xs:documentation>Level crossing information associated with ETCS operations.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:LevelCrossingSystem"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="footpathCrossing" type="signalling:FootpathCrossing"/>
  <xs:complexType name="FootpathCrossing">
    <xs:annotation>
      <xs:documentation>A footpath crossing the railway.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackCrossing"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="configuration" type="signalling:Configuration"/>
  <xs:complexType name="Configuration">
    <xs:annotation>
      <xs:documentation>Configuration information.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToBaselineOfRuleSet" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:BaselineOfRuleSet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasConfigurationProperty" type="signalling:ConfigurationProperty"/>
          <xs:element maxOccurs="1" minOccurs="0" name="lastModifiedBy" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ModificationInformation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="refersToAnnotation" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Annotation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToBaselineDocument" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>In case the time stamp does not uniquely define the applied set of rules and regulations, an attachment/document describing the applicable implementation can be referred to.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Attachment</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToOperationalLocality" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:OperationalLocality</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToTimeFrame" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Refers to time frame during which the asset is in operation.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TimeFrame</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
