<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:rtmCommon="http://www.railtopomodel.org/schemas/Common" xmlns:signalling="http://www.railtopomodel.org/schemas/EULYNX_Signalling" elementFormDefault="qualified" targetNamespace="http://www.railtopomodel.org/schemas/EULYNX_Signalling">
  <!--<dc:date>2020-10-06T17:19:00Z</dc:date><dc:creator>rtm2xsd 1.8.5</dc:creator><dc:source>Model , XMI 2.4.1, EA 13</dc:source>-->
  <xs:import namespace="http://www.railtopomodel.org/schemas/Common" schemaLocation="Common.xsd"/>
  <xs:simpleType name="ProjectPhaseTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="toBuild">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="asBuilt">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="tiltingArea" type="signalling:TiltingArea"/>
  <xs:complexType name="TiltingArea">
    <xs:annotation>
      <xs:documentation>Area equipped with a train protection system for tilting trains.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TpArea"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="underTrackCrossing" type="signalling:UnderTrackCrossing"/>
  <xs:complexType name="UnderTrackCrossing">
    <xs:annotation>
      <xs:documentation>A cable duct for guiding cables crossing underneath the track. Mostly a pipe.
UTX joins two cable pits, as per the cable duct topology.
&lt;ul&gt;
 &lt;li&gt;en-UK: UTX&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;de-DE: Kabelrohr&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tpwsTrain" type="signalling:TpwsTrain"/>
  <xs:complexType name="TpwsTrain">
    <xs:annotation>
      <xs:documentation>Category for trains equipped with Train Protection and Warning System.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrainCategory">
        <xs:attribute name="type" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Field for the user-defined train category.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsPositionReportParametersArea" type="signalling:EtcsPositionReportParametersArea"/>
  <xs:complexType name="EtcsPositionReportParametersArea">
    <xs:annotation>
      <xs:documentation>An area where a given set of position report parameters applies.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="isLocatedAt" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>rtmCommon:AreaLocation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="t_cycloc" type="xs:int">
        <xs:annotation>
          <xs:documentation>ETCS variable T_CYCLOC </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="aspectProving" type="signalling:AspectProving"/>
  <xs:complexType name="AspectProving">
    <xs:annotation>
      <xs:documentation>If a signal fails to show a given aspect, then another signal is forced to show another, typically more restrictive, aspect.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="bestAllowedAspect." type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>When the check fails, this is the best allowed signal/message combination.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:SignalAndMessage</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="checksAspect" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The aspect proving relation checks this signal/message combination. </xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:SignalAndMessage</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="systemState" type="signalling:SystemState"/>
  <xs:complexType abstract="true" name="SystemState">
    <xs:annotation>
      <xs:documentation>A set of (asset,state) pairs that describes the state of the signalling system. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="definedByAssetAndState" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to a set of (asset, state) tuples.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:AssetAndState</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsOverlap" type="signalling:EtcsOverlap"/>
  <xs:complexType name="EtcsOverlap">
    <xs:annotation>
      <xs:documentation>A stretch of track beyond EoA.
The RBC can release the ETCS overlap when it deems that the approaching train is slow enough such that overshoot is sufficiently unlikely.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="endsAtLocation" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>Maps to D_OL.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasAllowedTrainCategorySpeed" type="signalling:EtcsTrainCategorySpeed">
        <xs:annotation>
          <xs:documentation>The train in this category is allowed to approach the end of overlap with this release speed.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasOverlapTimer" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Maps to T_OL. If not given, overlap has infinite validity.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasTimerStartLocation" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>Maps to D_STARTOL. The location of the timer start is in advance of the EoA.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="automaticReleaseSpeedCalculation" type="xs:boolean" use="required">
      <xs:annotation>
        <xs:documentation>True: the release speed is calculated automatically.
False: the allowed train category speed provides the release speed.</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="cabinetGroup" type="signalling:CabinetGroup"/>
  <xs:complexType name="CabinetGroup">
    <xs:complexContent>
      <xs:extension base="signalling:Cabinet">
        <xs:attribute name="numberOfCabinets" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation>The number of cabinets (aka cases for NR) that are logically grouped together. Typically close together. 
</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="controlledTrackAsset" type="signalling:ControlledTrackAsset"/>
  <xs:complexType name="ControlledTrackAsset">
    <xs:annotation>
      <xs:documentation>Controlled track assets is the subset of track assets that 
&lt;ul&gt;
 &lt;li&gt;are &lt;b&gt;controlled &lt;/b&gt;by, and/or&lt;/li&gt;
 &lt;li&gt;are &lt;b&gt;powered &lt;/b&gt;by, and/or&lt;/li&gt;
 &lt;li&gt;&lt;b&gt;exchange &lt;/b&gt;information with a controller.&lt;/li&gt;
&lt;/ul&gt;
There has to be one instance of this class for each associated track asset instance.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToActiveSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ActiveSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToCompoundLogicalObject" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>this compound logical object is an active track asset</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:CompoundLogicalObject</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToILIntegratedLevelCrossing" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Dictates the control relation between the controlling interlocking and the level crossing system.
The level crossing is a controlled track asset and exchanges information/energy with the IL.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:InterlockingIntegratedLevelCrossing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToMovableElement" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>This movable element is a controlled track asset.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToReleaseInstrument" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The release instrument is a controlled track asset. In very rare cases, a release instrument doesn't feed back information to the interlocking; the key can only be released when a local condition is satisfied.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ReleaseInstrument</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToTpsDevice" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Stand-alone TP devices are not referred to by the controlled track asset class.
A self-powered TP device is not a controlled track asset but if the device has some control relation with a controller, for instance reporting health status, it is a controlled track asset.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TpsDevice</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToTrainDetectionSection" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TdsSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToVehiclePassageDetector" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>this vehicle passage detector is an active track asset</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:VehiclePassageDetector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="getsBackupPowerFrom" type="signalling:PowerSupplyTypes">
            <xs:annotation>
              <xs:documentation>in rare cases, controlled track assets could have a back up power supply</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isPoweredBy" type="signalling:PowerSupplyTypes"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="reportsToDiagnosticsCollector" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Diagnostic data from the controlled track asset are channeled to this diagnostics collector.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:DiagnosticsCollector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="MaterialTypes">
    <xs:annotation>
      <xs:documentation>A list of material types.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="timber">
        <xs:annotation>
          <xs:documentation>wood</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="concrete">
        <xs:annotation>
          <xs:documentation>beton</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="steel">
        <xs:annotation>
          <xs:documentation>steel sleepers are incompatible with track circuits</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="synthetic">
        <xs:annotation>
          <xs:documentation>plastic
DE: Kunststoff
NL: kunststof
FR: mati&amp;#232;re plastique </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="baseObject" type="signalling:BaseObject"/>
  <xs:complexType abstract="true" name="BaseObject">
    <xs:annotation>
      <xs:documentation>The EULYNX base object inherits an identity from the RTM base object. This identity acts as an unique anchor that other objects can refer to. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="rtmCommon:BaseObject"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="tElementWithIDref">
    <xs:attribute name="ref" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>The XSD generator can be hard coded so that the ref attribute is restricted to the pattern (urn:uuid:)?[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}|\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\}</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="tpTransitionMarker" type="signalling:TpTransitionMarker"/>
  <xs:complexType name="TpTransitionMarker">
    <xs:annotation>
      <xs:documentation>A spot on the track where a train enters the control area of a another TP system.
de-DE: Bereichsgrenze.
nl-NL: puntafbakening van het beinvloedingsgebied.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TpMarker"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="PointIndicatorTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="backlit">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="reflective">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="sectionList" type="signalling:SectionList"/>
  <xs:complexType name="SectionList">
    <xs:annotation>
      <xs:documentation>List of sections included in the route body, ordered from entry to exit.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToRouteBody" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:RouteBody</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="hasSection" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TvpSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LeuRelativePositionTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="at">
        <xs:annotation>
          <xs:documentation>The cabinet is placed at or attached to the signal post.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="behind">
        <xs:annotation>
          <xs:documentation>The cabinet is behind the signal, viewed from the signal's application direction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="inFront">
        <xs:annotation>
          <xs:documentation>The cabinet is in front of the signal, viewed from the signal's application direction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="signalAndMessage" type="signalling:SignalAndMessage"/>
  <xs:complexType name="SignalAndMessage">
    <xs:annotation>
      <xs:documentation>A pair (signal, message). 
When instantiated, this provides a list of all possible (signal, message) pairs needed for constructing &lt;u&gt;signal plans&lt;/u&gt;. Pairs that are not in the list are disallowed. 

An entry in the list would be for instance "signal 1105 shows yellow flashing".
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:AssetAndState">
        <xs:sequence>
          <xs:element name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="showsMessage" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A signal transmits a meaningful message to the train. The message can vary with the aspect of the signal.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:AspectComposition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lineSideSpotDetector" type="signalling:LineSideSpotDetector"/>
  <xs:complexType name="LineSideSpotDetector">
    <xs:annotation>
      <xs:documentation>Line-side detector, typically used to detect dangerous situations. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:LineSideSpotDevice">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="isOfType" type="signalling:LineSideDetectorTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LocalOperationDeviceTypes">
    <xs:annotation>
      <xs:documentation>Constructional type of the local operation device.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="panel">
        <xs:annotation>
          <xs:documentation>A vertical panel.
de-DE: Tafel</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="console">
        <xs:annotation>
          <xs:documentation>An inclined panel.
de-DE: Pult
en-UK: part of the UI where the interactive UI elements are. 
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="column">
        <xs:annotation>
          <xs:documentation>A vertical column with integrated control elements.
de-DE: S&amp;#228;ule</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tablet">
        <xs:annotation>
          <xs:documentation>A small panel, can be portable.
de-DE: Tablett</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="leverFrame">
        <xs:annotation>
          <xs:documentation>A set of levers, typically used to throw switch blades and (dis-)engage point locks.
en-GB: ground frame</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation>de-DE: sonstige</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="blockElementConfiguration" type="signalling:BlockElementConfiguration"/>
  <xs:complexType name="BlockElementConfiguration">
    <xs:annotation>
      <xs:documentation>The configuration of the line block working.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="appliesToLineBlockElement" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:BlockElement</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasBlockNormalisingWarning" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>True if an acoustic warning sounds when the block is normalised.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="1" name="hasBlockSettingType" type="signalling:BlockSettingTypes">
        <xs:annotation>
          <xs:documentation>A block element sets the block in one or more of these ways.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasTrainOnLineWarning" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>True if an acoustic warning sounds when a train approaches on the block.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="objectStatus" type="signalling:ObjectStatus"/>
  <xs:complexType abstract="true" name="ObjectStatus">
    <xs:annotation>
      <xs:documentation>An object-configuration can reflect a particular status, typically in construction phases.
For instance, a signal can be disconnected so drivers disregard the signal. The signal would then own a configuration associated with an out-of-service status.
This abstract class reflects an object status and the IM can specialise the object status that describes national needs.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfigurationProperty"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="equipmentSupport" type="signalling:EquipmentSupport"/>
  <xs:complexType name="EquipmentSupport">
    <xs:annotation>
      <xs:documentation>Generalization of fixings and housings, defined as civil structures that can support (or house) signalling equipment. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="2" minOccurs="0" name="isCivilStructure" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to a civil structure defined in a different register of preexisting civil assets. E.g. a database of bridges, gantries, posts etc.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:CivilStructure</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isLocatedAt" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Complex equipment supports may have multiple spot locations, e.g. a gantry with two foundations on each side of the track. The gantry can also support multiple signals, each with its own spot location. 
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="supports" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Recursive relation expressing the fact that one type of equipment support can support/carry/house another type of equipment support. For instance: "a foundation supports a control room that houses a cabinet".</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EquipmentSupport</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="boundaryElement" type="signalling:BoundaryElement"/>
  <xs:complexType name="BoundaryElement">
    <xs:annotation>
      <xs:documentation>The exit- or entry-element where a transition is ordered. This is a facade to a signal or balise group.
de-DE: Grenzdatenpunkt oder Einstiegssignal nach ESG</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="refersToEtcsBg" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>Refers to ETCS balise group.
The actual technical transition can have an offset with respect to the balise group.
Can be a balise group that acts as Last Relevant Balise Group (LRBG) at a transition ordered by the RBC.
de-DE: Grenzdatenpunkt
</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:EtcsBaliseGroup</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="refersToSignal" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>Reference to a signal at the transition.
The actual technical transition can have an offset with respect to the signal position (i.e. "early" or "late" transition)
de-DE: Einstiegssignal. Bei Einstieg ohne bekanntes r&amp;#252;ckwertiges Signal ist hier kein Signal vorgegeben.
</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="selfSupervisedLevelCrossing" type="signalling:SelfSupervisedLevelCrossing"/>
  <xs:complexType name="SelfSupervisedLevelCrossing">
    <xs:annotation>
      <xs:documentation>Warns and/or closes the barriers when activated by train approach detection. When this function fails, it guarantees to report failure to the interlocking.
Also known as autonomous level crossing.
de-DE: F&amp;#252;-B&amp;#220;, Fern&amp;#252;berwachter Bahn&amp;#252;bergang</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:InterlockingIntegratedLevelCrossing"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="signalMessageRelation" type="signalling:SignalMessageRelation"/>
  <xs:complexType name="SignalMessageRelation">
    <xs:annotation>
      <xs:documentation>A signal-message relation is composed of two pairs master(signal,message) and dependent(signal,message).
The message of the master signal dictates the message of the dependent signal.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="appliesToRoute" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The route body to which the signal-message relation applies.
The route body from dependent signal to master signal may contain points.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:RouteBody</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="entrySpeed" type="rtmCommon:Speed">
            <xs:annotation>
              <xs:documentation>The speed signalled by the entry signal, aka slave signal. I.e. the speed that the train must respect when passing the entry signal. Also known as passing speed.
This signalled speed can be supervised by an ATP system.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="exitSpeed" type="rtmCommon:Speed">
            <xs:annotation>
              <xs:documentation>Signalled maximum speed at exit signal (aka target or master signal)</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasDependentMessage" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The aspects of the dependent signal follow the master.
For instance: when the master indicates  "At Danger" the dependent signal indicates "Caution".</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasEndSectionTimer" type="rtmCommon:Duration">
            <xs:annotation>
              <xs:documentation>The end-section of a route is the section between the closed route exit signal and the previous worker signal. 
The interlocking revokes (part of) the route when this timer expires.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasMasterMessage" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The master signal dictates the message that a dependent signal can show.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="representsSpeedProfile" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>In speed signalling systems, the aspect relation between signals represents a speed profile that the train must respect. Note that the speed profile is attached to a master and worker signal.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SpeedProfile</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lightSignal" type="signalling:LightSignal"/>
  <xs:complexType name="LightSignal">
    <xs:annotation>
      <xs:documentation>A signal with lamps that transmit information to the driver. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ActiveSignal">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLuminosity" type="signalling:LuminosityTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="conditionAffectingSignal" type="signalling:ConditionAffectingSignal"/>
  <xs:complexType name="ConditionAffectingSignal">
    <xs:annotation>
      <xs:documentation>Condition required for the signal to show a given aspect, read from a compound logical object or a release instrument.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:RouteCondition">
        <xs:sequence>
          <xs:element name="affectsSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Signal that may open to the given aspect if the conditions in the route relation evaluate to true.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tpsSpotDevice" type="signalling:TpsSpotDevice"/>
  <xs:complexType name="TpsSpotDevice">
    <xs:annotation>
      <xs:documentation>A TPS device of limited length.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TpsDevice"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="dispatchPosition" type="signalling:DispatchPosition"/>
  <xs:complexType name="DispatchPosition">
    <xs:annotation>
      <xs:documentation>The spot from where trains can be dispatched, i.e. the spot where the dispatcher can sight train(s) and signals. Extra equipment like plungers can be installed here. 
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="isLocatedAt" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="CrossingTypes">
    <xs:annotation>
      <xs:documentation>Types of crossings.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="simple">
        <xs:annotation>
          <xs:documentation>A crossing without slip junctions.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="singleSlip">
        <xs:annotation>
          <xs:documentation>A single slip junction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="doubleSlip">
        <xs:annotation>
          <xs:documentation>A double slip junction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="RouteTractionTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="fullyElectrified">
        <xs:annotation>
          <xs:documentation>Suitable for electric traction.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="notFullyElectrified">
        <xs:annotation>
          <xs:documentation>The route (or parts of it) is not electrified. This route is permissible for non-electric vehicles only.
&lt;ul&gt;
 &lt;li&gt;Germany: F-Bedienung&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;NL: Dieseltractie&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;France: Thermique&lt;/li&gt;
&lt;/ul&gt;
Note: this is a static property and does not take into account whether traction power can be switched off.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="centralInterlocking" type="signalling:CentralInterlocking"/>
  <xs:complexType name="CentralInterlocking">
    <xs:annotation>
      <xs:documentation>The part of the interlocking where the core route logic runs. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Interlocking"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="redundancyLayerConfiguration" type="signalling:RedundancyLayerConfiguration"/>
  <xs:complexType name="RedundancyLayerConfiguration">
    <xs:annotation>
      <xs:documentation>Configuration parameters of the EULYNX RaSTA redundancy layer. Table 7.3</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="hasCheckCode" type="signalling:CheckCodeTypes"/>
      <xs:element name="hasMessagesCounters" type="signalling:RedundancyMessageCounters"/>
      <xs:element name="hasTseq" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Time period indicating how long a message is buffered that was received outside the sequence (DeferTime, clause 6.6).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="nPhysicalChannels" type="xs:int">
        <xs:annotation>
          <xs:documentation>Number of physical transport channels (clause 6.1). Default is 2.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="slabTrack" type="signalling:SlabTrack"/>
  <xs:complexType name="SlabTrack">
    <xs:annotation>
      <xs:documentation>Concrete, ballastless track bed.
DE: feste Fahrbahn</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Trackway">
        <xs:attribute name="isFloating" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>The concrete slab to which the rails are affixed is supported by a flexible layer that attenuates elastic waves.
Need not to be filled in if irrelevant to signalling or unknown.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tcConnector" type="signalling:TcConnector"/>
  <xs:complexType abstract="true" name="TcConnector">
    <xs:annotation>
      <xs:documentation>A connector used by track circuits.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAsset">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="connectsToRail" type="signalling:LeftRight">
            <xs:annotation>
              <xs:documentation>Denotes to which rail to connect. Given with respect to the orientation of the topological linear element.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isConnectedToRailAt" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Describes where the TcConnector (or Cable) is connected to the rail, not the dimensions of the connector itself.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="cableType" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Description of the type of cable according to cable book. For instance:
&lt;ul&gt;
 &lt;li&gt;DE: 120 Cu, 240 Al. &lt;/li&gt;
 &lt;li&gt;UK: cable type 5.&lt;/li&gt;
 &lt;li&gt;NL: kabel 50 mm2 koper of 70 mm2 aluminium.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="numberOfStrands" type="xs:int" use="optional">
          <xs:annotation>
            <xs:documentation>Number of strands in a cable. 
Used in DE.
DE: Verdrillte Seilen. 
NL: aantal aders.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="EtcsBaselines">
    <xs:annotation>
      <xs:documentation>The ETCS baselines</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="B2">
        <xs:annotation>
          <xs:documentation>Baseline 2, also known as SRS 2.3.0d 
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="B3">
        <xs:annotation>
          <xs:documentation>Baseline 3, which includes Limited Supervision</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="B4">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="signalAspectDegradation" type="signalling:SignalAspectDegradation"/>
  <xs:complexType name="SignalAspectDegradation">
    <xs:annotation>
      <xs:documentation>Provides signal aspect degradation information that is associated with a given light signal.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToActiveAspectSet" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Applies to this set of active aspects.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ActiveAspectSet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="appliesToLightSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LightSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="degradesToActiveAspectSet" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The active aspect set shown when the commanded set of active aspects can't be shown.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ActiveAspectSet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="isMostRestrictive" type="xs:boolean" use="required">
          <xs:annotation>
            <xs:documentation>True: the active aspect set is the most restrictive that the signal shows:
&lt;ul&gt;
 &lt;li&gt;when not controlled by the interlocking, e.g. in case of failed communication between interlocking and signal controller.&lt;/li&gt;
 &lt;li&gt;during system start.&lt;/li&gt;
&lt;/ul&gt;
False: degrade to the given set of active aspects.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="proceedMessage" type="signalling:ProceedMessage"/>
  <xs:complexType name="ProceedMessage">
    <xs:annotation>
      <xs:documentation>Indicates that the train may proceed.

Note: the end user can specialise this class to adapt it to national needs.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:AspectComposition"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsAcknowledgementMode" type="signalling:EtcsAcknowledgementMode"/>
  <xs:complexType name="EtcsAcknowledgementMode">
    <xs:annotation>
      <xs:documentation>Defines the acknowledgement area in rear of the start of the required mode.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsAcknowledgement">
        <xs:attribute name="l_ackmamode" type="xs:float" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="routeExtremity" type="signalling:RouteExtremity"/>
  <xs:complexType abstract="true" name="RouteExtremity">
    <xs:annotation>
      <xs:documentation>A route extremity is an abstraction of either a route entry or a route exit.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="isAssociatedWithSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TcFeederReceiverTypes">
    <xs:annotation>
      <xs:documentation>The different types of track circuit feeders and receivers.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="receiver">
        <xs:annotation>
          <xs:documentation>UK: letter R
FR: letters Re(cepteur)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="transmitter">
        <xs:annotation>
          <xs:documentation>UK: letter F(eed)
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="receiverWithTuner">
        <xs:annotation>
          <xs:documentation>audio frequency receiver
UK: letters RX
FR: Re(cepteur)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="transmitterWithTuner">
        <xs:annotation>
          <xs:documentation>audio frequency transmitter 
FR: Em(etteur)
UK: TX</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="receiverWithLoop">
        <xs:annotation>
          <xs:documentation>A track side receiver attached to a loop in the track.
FR: reception sur boucle</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ResetStrategyTypes">
    <xs:annotation>
      <xs:documentation>The types of reset strategies.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="fc_c">
        <xs:annotation>
          <xs:documentation>FC-C, Force section status to clear, conditional</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="fc_p">
        <xs:annotation>
          <xs:documentation>FC-P, Force section status to clear, preparatory</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="fc_p_a">
        <xs:annotation>
          <xs:documentation>FC-P-A, Force section status to clear, preparatory, with acknowledgement</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="fc_u">
        <xs:annotation>
          <xs:documentation>FC-U, Force section status to clear, unconditional</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="synchedFlashing" type="signalling:SynchedFlashing"/>
  <xs:complexType abstract="true" name="SynchedFlashing">
    <xs:annotation>
      <xs:documentation>Two light aspects that flash in synch. 
The frequency and phase difference of flashing are given by national rules.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="2" minOccurs="2" name="appliesToAspect" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Aspect</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="depressionBar" type="signalling:DepressionBar"/>
  <xs:complexType name="DepressionBar">
    <xs:annotation>
      <xs:documentation>A mechanical bar that is depressed by a passing train wheel.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:VehicleSensor"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="shutInSidingKeyReleaseInstrument" type="signalling:ShutInSidingKeyReleaseInstrument"/>
  <xs:complexType name="ShutInSidingKeyReleaseInstrument">
    <xs:annotation>
      <xs:documentation>Key release instrument particular to shunting sidings on open line.
It isolates (or &lt;i&gt;shuts in&lt;/i&gt;) the shunting yard from the main running line.
de-DE: Ausweichanschlussstelle</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:KeyReleaseInstrument"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="leuSystem" type="signalling:LeuSystem"/>
  <xs:complexType name="LeuSystem">
    <xs:annotation>
      <xs:documentation>A Lineside Electronic Unit (LEU) system controls zero or more ETCS balises.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:FieldObjectController">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasLeuPort" type="signalling:LeuPort"/>
          <xs:element maxOccurs="1" minOccurs="0" name="isGovernedByMovableElement" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to a movable element that governs the LEU's output. Direct, typically a contact at the level of the point control or indirect via interlocking logic ??</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="2" minOccurs="0" name="isHousedInCabinet" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuCabinet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isRelatedToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The signal, SMB or light signal associated with the LEU. Typically used to generate the designation of the LEU.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lineSideLinearConstruction" type="signalling:LineSideLinearConstruction"/>
  <xs:complexType name="LineSideLinearConstruction">
    <xs:annotation>
      <xs:documentation>Technical line-side construction relevant to, but not planned by, signalling.
Facade for devices like
&lt;ul&gt;
 &lt;li&gt;Bridge&lt;/li&gt;
 &lt;li&gt;Tunnel&lt;/li&gt;
 &lt;li&gt;Noise protection barrier&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="isLocatedAt" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>rtmCommon:LinearLocation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="refersToCivilStructure" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The line-side device, e.g. a bridge, may be registered as a civil structure.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:CivilStructure</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="etcsBalise" type="signalling:EtcsBalise"/>
  <xs:complexType name="EtcsBalise">
    <xs:annotation>
      <xs:documentation>Eurobalise in an ETCS system.
The model type as defined by the manufacturer can be described in the &lt;a href="$feature://{3D2A1B2D-BBA0-4bce-8064-8F86DBA8783F}"&gt;&lt;font color="#0000ff"&gt;&lt;u&gt;Supplier Information class.&lt;/u&gt;&lt;/font&gt;&lt;/a&gt;
Variable N_PIG is inherited from attribute "positionInSystem".</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Balise">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="duplicatesEtcsBalise" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Indicates whether this balise is a duplicate of an adjacent balise.
Implements ETCS variable M_DUP.
Can be left out in early planning stages when this information is not yet known. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsBalise</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isControlledByLeuPort" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>An active balise is controlled by a port.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuPort</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableJointRack" type="signalling:CableJointRack"/>
  <xs:complexType name="CableJointRack">
    <xs:annotation>
      <xs:documentation>A rack in the field where cables arrive and cores are dispatched to endpoints.
DE: Kabelverbindungsgestell, streckenseitiger Kabelverteiler, d.h. nicht im Stw-Gebaeude.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableJointInstallation"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="mechanicalMechanism" type="signalling:MechanicalMechanism"/>
  <xs:complexType name="MechanicalMechanism">
    <xs:annotation>
      <xs:documentation>Worked by means of wires or rods, at some distance.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Mechanism"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="signalFrameComponent" type="signalling:SignalFrameComponent"/>
  <xs:complexType name="SignalFrameComponent">
    <xs:annotation>
      <xs:documentation>E.g. Lamp, light spot, or indicator that is part of a signal frame.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasAspect" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Aspect</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableJointInstallation" type="signalling:CableJointInstallation"/>
  <xs:complexType name="CableJointInstallation">
    <xs:annotation>
      <xs:documentation>Device where one set of &lt;i&gt;n&lt;/i&gt; cables/wires terminates and connects to another set of &lt;i&gt;m&lt;/i&gt; cables/wires.
Number &lt;i&gt;n&lt;/i&gt; is not necessarily equal to &lt;i&gt;m&lt;/i&gt;.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="containsCableJoint" type="signalling:CableJointRelation"/>
          <xs:element name="isHousedIn" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The housing of the cable joint installation tends to dictate which class to use. The function is always the same, i.e. joining, terminating cables and wires.
A cable termination rack is housed in the IL room or in a cabinet in the IL room.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableRouteExit" type="signalling:CableRouteExit"/>
  <xs:complexType name="CableRouteExit">
    <xs:annotation>
      <xs:documentation>Where cable can exit or enter the duct, typically a simple hole.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableRouteJunction"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsGradientChangeMarker" type="signalling:EtcsGradientChangeMarker"/>
  <xs:complexType name="EtcsGradientChangeMarker">
    <xs:annotation>
      <xs:documentation>Marks a &lt;i&gt;spot location&lt;/i&gt; from where a given gradient applies. Can be associated with a gradient sign (UK).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsMarker">
        <xs:attribute name="g_a" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation>The position where the ETCS gradient changes. This is a gradient that has been derived by engineering rules from the slopes according to ETCS subset-027-3.11.12.4</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="q_gdir" type="xs:int" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="automatic" type="signalling:Automatic"/>
  <xs:complexType name="Automatic">
    <xs:annotation>
      <xs:documentation>Signal is part of an autoworking route. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="isOfAutomaticType" type="signalling:AutomaticTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="TpsPowerSupplyTypes">
    <xs:annotation>
      <xs:documentation>Power supply types used for TPS devices.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="interlocking">
        <xs:annotation>
          <xs:documentation>IL supplies power.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="battery">
        <xs:annotation>
          <xs:documentation>battery only.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="solar">
        <xs:annotation>
          <xs:documentation>solar (always have batteries included).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="specialTrackGauge" type="signalling:SpecialTrackGauge"/>
  <xs:complexType name="SpecialTrackGauge">
    <xs:annotation>
      <xs:documentation>Area with non-standard gauge, i.e. other than 1435 mm.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:SpecialGauge">
        <xs:sequence>
          <xs:element maxOccurs="2" minOccurs="1" name="hasValue" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>Dual gauge has two values for the distance between the rails, e.g. in CH.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="signalLocation" type="signalling:SignalLocation"/>
  <xs:complexType name="SignalLocation">
    <xs:annotation>
      <xs:documentation>Description of the area where the signal is located, to help maintenance workers to find the signal in the field.
Can be an informal description (e.g. near junction x) or depend on national regulations (e.g. DE Bezeichnung nach Ril 100).
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:attribute name="description" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Textual description of the location. Not to be confounded with geo-localisation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsEndOfAuthority" type="signalling:EtcsEndOfAuthority"/>
  <xs:complexType name="EtcsEndOfAuthority">
    <xs:annotation>
      <xs:documentation>ETCS End Of Authority (EoA) marks the end of an ETCS Movement Authority.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="hasDistanceInRearOfSignal" type="rtmCommon:Length">
        <xs:annotation>
          <xs:documentation>The ETCS EoA is located at a given distance in rear of the signal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasEtcsDangerpoint" type="signalling:EtcsDangerpoint">
        <xs:annotation>
          <xs:documentation>Maps to D_DP.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasOverlap" type="signalling:EtcsOverlap"/>
      <xs:element maxOccurs="1" minOccurs="0" name="hasPermittedSpeed" type="rtmCommon:Speed">
        <xs:annotation>
          <xs:documentation>The speed limit at the EoA.
If not given, the value is considered unknown.
Known as V_EMA in ETCS 3.5.0; formerly known as V_LOA.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="localOperatingAreaUiElement" type="signalling:LocalOperatingAreaUiElement"/>
  <xs:complexType name="LocalOperatingAreaUiElement">
    <xs:annotation>
      <xs:documentation>Associates a button, switch or pull cord with a given function allowing local staff to interact with a local operating zone.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="affectsLocalOperatingZone" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LocalOperatingZone</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="appliesToLocalOperationUiElement" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LocalOperationUiElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasFunctionType" type="signalling:UiElementFunctionTypes">
            <xs:annotation>
              <xs:documentation>The button has a function given by this enumeration.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="mainSignalProtectedLevelCrossing" type="signalling:MainSignalProtectedLevelCrossing"/>
  <xs:complexType name="MainSignalProtectedLevelCrossing">
    <xs:annotation>
      <xs:documentation>Level crossing system integrated with a route and protected by a signal, normally a main signal. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:InterlockingIntegratedLevelCrossing"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="PlatformAccessTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="staircase">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="lift">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ramp">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="walkway">
        <xs:annotation>
          <xs:documentation>a track crossing at the level of the rails, for accessing a platform situated between two tracks. 
DE: Resi, Reisendensicherungsanlage (von Bahnpersonal gesperrt und in Stellwerkslogik eingespeist).
UK: barrow crossing (not in use anymore)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="escalator">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="etcsTelegramConditionRelation" type="signalling:EtcsTelegramConditionRelation"/>
  <xs:complexType name="EtcsTelegramConditionRelation">
    <xs:annotation>
      <xs:documentation>Relates a set of telegrams that is sent when a given set of conditions applies.
Used for switchable balises.

The SRS versions allows selecting telegrams of a given version.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesToEtcsTelegram" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsTelegram</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="sentConditionally" type="signalling:EtcsCondition">
            <xs:annotation>
              <xs:documentation>Stipulates a condition under which an ETCS telegram is set.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="srsSubversion" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>The subversion of subset-026-7
E.g. Maintenance Release 1</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="srsVersion" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>The version of Subset-026-7</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tiltingTrain" type="signalling:TiltingTrain"/>
  <xs:complexType name="TiltingTrain">
    <xs:annotation>
      <xs:documentation>Category for trains equipped with a tilting mechanism.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrainCategory"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="leuModule" type="signalling:LeuModule"/>
  <xs:complexType name="LeuModule">
    <xs:annotation>
      <xs:documentation>Hardware module for controlling Eurobalises.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="getsInputFromLeuModule" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>An LEU can get input from another LEU module.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuModule</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="getsInputFromLeuSystem" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The LEU module gets input from a LEU system, &lt;i&gt;other than &lt;/i&gt;the LEU system of which it is part.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="getsInputFromTrackAsset" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The LEU module gets input from a track asset, one &lt;i&gt;or more &lt;/i&gt;of
&lt;ul&gt;
 &lt;li&gt;signal&lt;/li&gt;
 &lt;li&gt;turnout&lt;/li&gt;
 &lt;li&gt;TPS device&lt;/li&gt;
 &lt;li&gt;controller&lt;/li&gt;
 &lt;li&gt;level crossing system&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TrackAsset</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasLeuPort" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuPort</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="isClassifiedAs" type="signalling:LeuModuleClassificationTypes">
            <xs:annotation>
              <xs:documentation>The LEU has a functional classification.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="isHousedInLeuCabinet" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The LEU module is housed in a cabinet</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuCabinet</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="isPartOfLeuSystem" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The LEU module plugs into a LEU system.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LeuSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="diagnosticsCollector" type="signalling:DiagnosticsCollector"/>
  <xs:complexType name="DiagnosticsCollector">
    <xs:annotation>
      <xs:documentation>Service function that processes and channels diagnostics data.
Can be a separate hardware component or virtualised software such as a OPC-UA server.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="hasDiagnosticsConfiguration" type="signalling:DiagnosticsOpcUaConfiguration"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="servesDiagDataToCollector" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A lower level collector serves diagnostics data to a collector that aggregates data.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:DiagnosticsCollector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsCesPointCondition" type="signalling:EtcsCesPointCondition"/>
  <xs:complexType name="EtcsCesPointCondition">
    <xs:annotation>
      <xs:documentation>The CES is issued when point element(s) is in the given position. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsCesCondition">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesWhenPointElementInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>a CES applies when the point element is in the given position.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="routeConflictOverlap" type="signalling:RouteConflictOverlap"/>
  <xs:complexType name="RouteConflictOverlap">
    <xs:annotation>
      <xs:documentation>The route conflicts with overlap of the other route.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConflictingRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="bufferSensor" type="signalling:BufferSensor"/>
  <xs:complexType name="BufferSensor">
    <xs:annotation>
      <xs:documentation>A friction buffer stop can be equipped with a sensor that detects when an impact has shifted the buffer.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:VehicleSensor">
        <xs:sequence>
          <xs:element name="monitorsBufferStop" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:BufferStop</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tpMarker" type="signalling:TpMarker"/>
  <xs:complexType abstract="true" name="TpMarker">
    <xs:annotation>
      <xs:documentation>A mark from where Train Protection applies a (different) restriction. 
Use the inherited Description to annotate the ATP area demarcation (entry or exit point)
E.g. "entry towards tracks 7-14"
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ChangeMarker">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="fromNtc" type="signalling:NTC">
            <xs:annotation>
              <xs:documentation>The NTC from which a transit takes place.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="transitFromEtcsLevel" type="signalling:EtcsLevelTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="overlapRelease" type="signalling:OverlapRelease"/>
  <xs:complexType name="OverlapRelease">
    <xs:annotation>
      <xs:documentation>Unlocks the elements in the overlap.
The IL may release the overlap
&lt;ul&gt;
 &lt;li&gt;When the approaching train &lt;i&gt;has plausibly stopped&lt;/i&gt;&lt;u&gt; &lt;/u&gt;before the exit signal. In this case, the overlap release is delayed by a given duration: the timer is triggered when the train occupies or vacates a given section.&lt;/li&gt;
 &lt;li&gt;When the approaching train &lt;i&gt;positively confirms to have stopped&lt;/i&gt; (e.g. by means of an ETCS position report). In this case, the release is immediate or with a short delay.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="hasOverlapReleaseDelay" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Time that expires before the interlocking releases the overlap.
DE: D-Weg-Aufl&amp;#246;severz&amp;#246;gerung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="isTriggeredBy" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The section that triggers the (delayed) release of the overlap. German Zielgleis, UK berthing track. 
Condition for release is found in overlap release condition.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:TvpSection</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="overlapReleaseCondition" type="signalling:OverlapReleaseConditionTypes"/>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="NonStopMovementAllowedTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="disallowed">
        <xs:annotation>
          <xs:documentation>The train is to stop, only after which the signal is opened for a follow-on route.
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="allowed">
        <xs:annotation>
          <xs:documentation>A route beyond the signal may be called together with the previous route so that the train can proceed without stopping.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="nextIsShunting">
        <xs:annotation>
          <xs:documentation>The train is allowed to proceed with a shunting route.

Ist betrieblich die Weiterfahrt einer Zugfahrt als Rangierfahrt zu gestatten </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="directionAuthorityDependentOverlap" type="signalling:DirectionAuthorityDependentOverlap"/>
  <xs:complexType name="DirectionAuthorityDependentOverlap">
    <xs:annotation>
      <xs:documentation>The overlap can only be activated when the block accepts the direction in which the route and thus the overlap are set.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToOverlap" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Overlap</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lineBlockInformation" type="signalling:LineBlockInformation"/>
  <xs:complexType name="LineBlockInformation">
    <xs:annotation>
      <xs:documentation>Set of general attributes for the block line, viewed from a station.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isDesignedForBrakingDistance" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>The block line is designed for the given braking distance.
Only used by DB for validation purposes.
de-DE: Bremsweg als Grundlage f&amp;#252;r die Dimensionierung der zugeh&amp;#246;rigen Strecke.
SNCF documents show the signals and the distances between signals, ergo no need for braking distance parameter. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isDesignedForSpeed" type="rtmCommon:Speed">
            <xs:annotation>
              <xs:documentation>The speed for which the line is designed.
Only used by DB for validation purposes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="leadsToNextMainOperationalLocality" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Indicates the next (major) junction that can be reached when travelling down the block line.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:OperationalLocality</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="leadsToNextOperationalLocality" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Indicates the next operational locality that is reached when travelling &lt;i&gt;from &lt;/i&gt;the block element down the block line.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:OperationalLocality</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="electrificationSegment" type="signalling:ElectrificationSegment"/>
  <xs:complexType abstract="true" name="ElectrificationSegment">
    <xs:annotation>
      <xs:documentation>Electrification segment with information relevant to signalling.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackProperty"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cablePipe" type="signalling:CablePipe"/>
  <xs:complexType name="CablePipe">
    <xs:annotation>
      <xs:documentation>A pipe conduct for a cable, not being an UTX.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsSpeedChangeMarker" type="signalling:EtcsSpeedChangeMarker"/>
  <xs:complexType name="EtcsSpeedChangeMarker">
    <xs:annotation>
      <xs:documentation>From this spot and in the given direction, a speed applies.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsMarker">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasCantDeficiencySpeed" type="signalling:CantDeficiencySpeed"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasEtcsTrainCategorySpeed" type="signalling:EtcsTrainCategorySpeed">
            <xs:annotation>
              <xs:documentation>has speed depending on ETCS train category</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasSpeedChangeCondition" type="signalling:EtcsSpeedChangeCondition"/>
        </xs:sequence>
        <xs:attribute name="q_diff" type="xs:int" use="required"/>
        <xs:attribute name="q_front" type="xs:boolean" use="required"/>
        <xs:attribute name="v_static" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation>Maps to V_STATIC, basic SSP.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="nonStoppingArea" type="signalling:NonStoppingArea"/>
  <xs:complexType name="NonStoppingArea">
    <xs:annotation>
      <xs:documentation>Area where stopping isn't allowed.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackConditionArea">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="description" type="xs:string">
            <xs:annotation>
              <xs:documentation>Describes or names the area, e.g. tunnel or bridge, that is the reason for this prohibition. </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="handOperatedMechanism" type="signalling:HandOperatedMechanism"/>
  <xs:complexType name="HandOperatedMechanism">
    <xs:annotation>
      <xs:documentation>Worked by hand locally.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Mechanism"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="routeProperty" type="signalling:RouteProperty"/>
  <xs:complexType abstract="true" name="RouteProperty">
    <xs:annotation>
      <xs:documentation>Base class for additional route properties.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToRoute" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Route</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="causesSpeedRestriction" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A route with a given property can cause a speed restriction.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SpeedSegment</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="assetControlRelation" type="signalling:AssetControlRelation"/>
  <xs:complexType name="AssetControlRelation">
    <xs:annotation>
      <xs:documentation>Association class informing about the control relation between a controller A and a controlled track asset B.
The direction of flow A&amp;lt;-&amp;gt;B is inherited from the control relation.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ControlRelation">
        <xs:sequence>
          <xs:element name="endPointAController" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Controller</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="endPointBTrackAsset" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Active track assets is powered by and/or exchange information with a controller.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ControlledTrackAsset</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="movableElementAndPosition" type="signalling:MovableElementAndPosition"/>
  <xs:complexType name="MovableElementAndPosition">
    <xs:annotation>
      <xs:documentation>A pair (movable element, position). </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:AssetAndState">
        <xs:sequence>
          <xs:element name="refersToMovableElement" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="normal" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>If normal=true: the given position is the &lt;b&gt;normal &lt;/b&gt;position (the other position is the &lt;b&gt;reverse &lt;/b&gt;position).
If this attribute isn't given, then neither normal nor reverse position is defined.
</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="preferred" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>If preferred=true: the given position is the &lt;b&gt;preferred &lt;/b&gt;position (the position to which the movable element returns when no longer claimed by a route).
If this attribute isn't given, then there is no preferred position.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsSpeedSignalCondition" type="signalling:EtcsSpeedSignalCondition"/>
  <xs:complexType name="EtcsSpeedSignalCondition">
    <xs:annotation>
      <xs:documentation>The speed change applies when the given signal shows the given message. The message is composed by a (number of) aspect(s).
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsSpeedChangeCondition">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesWhenSignalShowsMessage" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SignalAndMessage</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tempShuntingCondition" type="signalling:TempShuntingCondition"/>
  <xs:complexType name="TempShuntingCondition">
    <xs:annotation>
      <xs:documentation>The ETCS telegram is sent when the temporary shunting zone is taken by local staff.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsCondition">
        <xs:sequence>
          <xs:element name="activeWhenTempShuntingAreaTaken" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TempShuntingArea</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="physicalPowerSupplyModule" type="signalling:PhysicalPowerSupplyModule"/>
  <xs:complexType name="PhysicalPowerSupplyModule">
    <xs:annotation>
      <xs:documentation>A physical power supply field module.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:PowerSupplyModule">
        <xs:sequence>
          <xs:element name="isHousedIn" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The housing of the power supply module</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isPsModuleType" type="signalling:PowerSupplyModuleType"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="CheckCodeTypes">
    <xs:annotation>
      <xs:documentation>Check code configuration parameter of the redundancy layer (Clause 6.3.6).</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="optionA">
        <xs:annotation>
          <xs:documentation>None</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionB">
        <xs:annotation>
          <xs:documentation>CRC32 with polynomial 0x EE5B42FD</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionC">
        <xs:annotation>
          <xs:documentation>CRC32 with polynomial 0x 1EDC6F41</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionD">
        <xs:annotation>
          <xs:documentation>CRC16 with polynomial 0x 1021</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="optionE">
        <xs:annotation>
          <xs:documentation>CRC16 with polynomial 0x 8005</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ioController" type="signalling:IoController"/>
  <xs:complexType name="IoController">
    <xs:annotation>
      <xs:documentation>Controller for receiving binary inputs and sending binary outputs.
&lt;ul&gt;
 &lt;li&gt;Inputs, typically received from relay contacts, are packed into telegrams and sent to the interlocking.&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;Telegrams received from the interlocking are converted to outputs.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:FieldObjectController"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tpsDeviceProperties" type="signalling:TpsDeviceProperties"/>
  <xs:complexType name="TpsDeviceProperties">
    <xs:annotation>
      <xs:documentation>Properties of a TPS device.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToTpsDevice" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>the properties apply to this TPS device</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TpsDevice</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLength" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>e.g. length of infill loop. 
Balises, magnets, crocodiles or TPWS loops are considered point devices without a length.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasPowerSupplyType" type="signalling:TpsPowerSupplyTypes">
            <xs:annotation>
              <xs:documentation>allows defining the power source of TPS device, &lt;i&gt;even&lt;/i&gt; &lt;i&gt;not-controlled ones. &lt;/i&gt;</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="AutomaticTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="autoworking">
        <xs:annotation>
          <xs:documentation>de-DE: Signalselbststellbetrieb </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="subjectToARS">
        <xs:annotation>
          <xs:documentation>Controlled by automatic route setting system 
de-DE: Zuglenkung).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="electrificationOverlap" type="signalling:ElectrificationOverlap"/>
  <xs:complexType name="ElectrificationOverlap">
    <xs:annotation>
      <xs:documentation>Section where overhead lines overlap. When a train stops in the overlap, chances are that current runs through and damages the pantograph. Trains should therefore not stop here. 
fr-FR: sectionnement critique
nl-NL: open spaninrichting.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ElectrificationSegment">
        <xs:attribute name="type" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>Description of the type of overlap like booster, auto, traction mounted auxiliary, normally open/closed switch.
Used for assessing where to place signals to minimise the risk of trains stopping in the overlap.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="trainRearConfirmation" type="signalling:TrainRearConfirmation"/>
  <xs:complexType name="TrainRearConfirmation">
    <xs:annotation>
      <xs:documentation>A switch or button allowing line-side staff to confirm that the rear of the train has cleared the block section.
de-DE: Zugschlussmeldung </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:LocalOperationUiElement"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableRouteJunction" type="signalling:CableRouteJunction"/>
  <xs:complexType name="CableRouteJunction">
    <xs:annotation>
      <xs:documentation>A physical node in the cable route network where cables can enter/exit a cable route. From this hub the cable route goes to controller, controlled track asset, cable distributor or housing.
This class can be instantiated if no appropriate specialised class is available, for instance at an early planning stage.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="1" name="isLocatedAt" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>Can be located on &lt;i&gt;1..* &lt;/i&gt;spot locations because the route junction is typically situated at the end or start of 1..* net elements. </xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="joinsCableDistributor" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:CableJointInstallation</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="joinsControlledTrackAsset" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>References a ControlledTrackAsset to which the cable is connected.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:ControlledTrackAsset</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="joinsController" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>References a controller to which the cable is connected.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:Controller</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="joinsHousing" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The housing on the cable route or where the cable route terminates.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:Housing</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="cableTerminationRack" type="signalling:CableTerminationRack"/>
  <xs:complexType name="CableTerminationRack">
    <xs:annotation>
      <xs:documentation>A rack in the relay room/signal box where field cables arrive and cores are dispatched to the interlocking or other systems.
de-DE: Kabelabschlussgestell im Stw Gebaeude</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableJointInstallation"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="overlap" type="signalling:Overlap"/>
  <xs:complexType name="Overlap">
    <xs:annotation>
      <xs:documentation>Overlap
de-DE: Durchrutschweg (D-Weg)
nl-NL: doorschietlengte</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:SafetyDistance">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="overlapRelease" type="signalling:OverlapRelease"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="requiresCrossingElementLockedInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:CrossingElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="requiresDerailerLockedInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:DerailerElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="requiresFlankProtection" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Used in those cases that rules are insufficient to describe flank protection
DB: not filled in because implemented by supplier by rules.
NR: only special cases are described.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:FlankProtection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="requiresMovableElementLocked" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Applies to points and crossings only, not to derailers.
The position of the referenced point can be derived from the corresponding LinearLocation of the Overlap.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="requiresPointElementLockedInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to a point element in a given position, proving types are available.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="speedRestrictionInApproach" type="rtmCommon:Speed">
            <xs:annotation>
              <xs:documentation>Speed restriction in approach to the closed exit signal. Typically depends on the length of overlap and is calculated according to national rules and regulations.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="speedThroughOverlap" type="rtmCommon:Speed">
            <xs:annotation>
              <xs:documentation>Permitted speed for trains travelling through the overlap.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="specialGauge" type="signalling:SpecialGauge"/>
  <xs:complexType abstract="true" name="SpecialGauge">
    <xs:annotation>
      <xs:documentation>Area(s) where a non-standard gauge applies. Standard gauges can be found in the national domain or indicated by reference drawings. 
Structure gauge is the envelope.
Track gauge is the distance between the rails. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackProperty"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="releaseInstrument" type="signalling:ReleaseInstrument"/>
  <xs:complexType name="ReleaseInstrument">
    <xs:annotation>
      <xs:documentation>A device that releases control of an asset.
nl-NL: Grendel</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAsset">
        <xs:sequence>
          <xs:element name="isHousedIn" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="releasesTrackAsset" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The release instrument releases the track asset so that it can be operated locally.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ControlledTrackAsset</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="standOutControl" type="signalling:StandOutControl"/>
  <xs:complexType name="StandOutControl">
    <xs:annotation>
      <xs:documentation>Indicates that a signal from a terminal track can be cleared even when the section in advance of that signal is occupied.
de-DE: Besetzte Ausfahrt</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="EulynxSciTypes">
    <xs:annotation>
      <xs:documentation>List of standard communication interfaces for carrying process data.
SCI: Standard Communication Interface
DPI: Data Process Interface</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ls">
        <xs:annotation>
          <xs:documentation>Interface to light signal</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="lx">
        <xs:annotation>
          <xs:documentation>Interface to external level crossing system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="lc">
        <xs:annotation>
          <xs:documentation>Interface to subsystem level crossing</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="p">
        <xs:annotation>
          <xs:documentation>Interface to point.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tds">
        <xs:annotation>
          <xs:documentation>Interface to train detection system.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="io">
        <xs:annotation>
          <xs:documentation>Interface to (binary) input/output</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="rbc">
        <xs:annotation>
          <xs:documentation>Interface to Radio Block Centre.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="ils">
        <xs:annotation>
          <xs:documentation>Interface to adjacent interlocking system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="cc">
        <xs:annotation>
          <xs:documentation>Interface to Command &amp;amp; Control system.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="tss">
        <xs:annotation>
          <xs:documentation>Interface to trackworker safety system</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="equipmentHouse" type="signalling:EquipmentHouse"/>
  <xs:complexType name="EquipmentHouse">
    <xs:annotation>
      <xs:documentation>A purpose-built trackside building housing signalling equipment.
de-DE: Schalthaus
nl-NL: relaishuis</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Building"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="flankProtection" type="signalling:FlankProtection"/>
  <xs:complexType name="FlankProtection">
    <xs:annotation>
      <xs:documentation>Flank protection prevents rolling stock from unintentionally entering the train's route body.
Flank protection is given when selected elements, notably points, prove a given state.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isProvidedByDerailerInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Flank protection is provided by derailers in position engaged.
Software should check that the data don't wrongfully state that flank protection is provided by non-engaged derailers. 
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:DerailerElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isProvidedByPointElementInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Flank protection is provided by points in a given position. 
This can include trap points. 
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isProvidedBySignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isRequestedByCrossingElementInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:CrossingElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isRequestedByLocalOperatingZoneLimit" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The limit of a local operating zone requests flank protection.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LocalOperatingZoneLimit</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isRequestedByLoz" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Flank protection is requested by the given local operating zone.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LocalOperatingZone</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isRequestedByPointElementInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isRequestedByToeOfPointElement" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Defines a flank protection case where the toe of a point is the requestor.
DB: used when flank protection request is transmitted via the toe of a point.

de-DE: Anforderung von der Weichenspitze</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="requiresSectionVacant" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SectionAndVacancy</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="powerOutputChannelProperties" type="signalling:PowerOutputChannelProperties"/>
  <xs:complexType name="PowerOutputChannelProperties">
    <xs:annotation>
      <xs:documentation>The parameters of a power output channel.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="appliesToPsModule" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:PowerSupplyModule</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="channelNumber" type="xs:int">
        <xs:annotation>
          <xs:documentation>The channel of the output on the power supply. Only given if the power supply has mutliple channels.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="maxPower" type="xs:int">
        <xs:annotation>
          <xs:documentation>Maximum power in Watts that the channel can constantly supply. 
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="maxPowerInterruptDuration" type="xs:float">
        <xs:annotation>
          <xs:documentation>The maximum duration during which this channel may not provide power.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="maxVoltage" type="xs:int">
        <xs:annotation>
          <xs:documentation>Upper limit of the tolerance, i.e. maximum voltage that is supplied by the channel.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="minVoltage" type="xs:int">
        <xs:annotation>
          <xs:documentation>Lower limit of the tolerance, i.e. minimum voltage that is supplied by the channel.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="nominalPower" type="xs:int">
        <xs:annotation>
          <xs:documentation>Nominal power in Watts that the channel supplies.
de-DE: Nennleistung</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="providesNominalVoltage" type="signalling:NominalVoltageTypes"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="trackCircuit" type="signalling:TrackCircuit"/>
  <xs:complexType name="TrackCircuit">
    <xs:annotation>
      <xs:documentation>&lt;p align="right" dir="rtl"&gt;A train detection system that detects train axles shunting the rails.&lt;/p&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TdsSection">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasProperties" type="signalling:TrackCircuitProperties"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasTcFeederReceiver" type="signalling:TcFeederReceiverElement">
            <xs:annotation>
              <xs:documentation>A track circuit has 1..4 elements for feeding or receiving signals. </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="cutSectionIndex" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>UK: index of the cut section track, e.g. 60G103&lt;u&gt;.1&lt;/u&gt;
NL: index van de technische sectie
DE: FMA Kaskade Bezeichnung bei technisch notwendiger Unterteilung eines Gleisabschnitts in mehr als einen Freimeldeabschnitt</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="individualAssesment" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>If true, the cut-section vacancy statuses are &lt;i&gt;assessed &lt;/i&gt;and reported to the interlocking &lt;i&gt;individually&lt;/i&gt;. 
If false, the interlocking receives the lumped vacant/occupied status of all cut-sections.
Only used in combination with cascaded, or cut sections.
DE: Einzelauswertung vorgesehen</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="safetyRetransmissionLayerConfiguration" type="signalling:SafetyRetransmissionLayerConfiguration"/>
  <xs:complexType name="SafetyRetransmissionLayerConfiguration">
    <xs:annotation>
      <xs:documentation>Configuration parameters of the EULYNX RaSTA Safety/Retransmission layer, Table 23.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="hasMessageCounters" type="signalling:SafetyMessageCounters"/>
      <xs:element name="hasTh" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Time period for sending heartbeats. The interval between successive messages proving that a communication link is still alive. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="hasTmax" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Maximum accepted age of a message.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="usesSafetyCode" type="signalling:RaStaSafetyCode">
        <xs:annotation>
          <xs:documentation>RaSTA safety code according to &amp;#167;5.3.11. Option 1 is to give no safety code. Option 2 is to preset the lower half of the MD4, option 3 is to preset lower and upper half of the MD4.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="etcsMarker" type="signalling:EtcsMarker"/>
  <xs:complexType name="EtcsMarker">
    <xs:annotation>
      <xs:documentation>An ETCS marker is a spot on the track where an event occurs or a property changes that is relevant to ETCS signalling:
&lt;ul&gt;
 &lt;li&gt;Change in National Values&lt;/li&gt;
 &lt;li&gt;Change of gradient&lt;/li&gt;
 &lt;li&gt;Change of speed&lt;/li&gt;
 &lt;li&gt;Change of track condition&lt;/li&gt;
 &lt;li&gt;Start of text display&lt;/li&gt;
&lt;/ul&gt;

An ETCS profile is an ordered sequence of markers.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TpMarker">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="appliesWhenPointElementInPosition" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The marker action applies only when the point element(s) are in the given position.
de-DE: Entscheidungsweiche</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="limitsTcArea" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The ETCS marker marks the entry into track condition area(s).</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TrackConditionArea</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="building" type="signalling:Building"/>
  <xs:complexType name="Building">
    <xs:annotation>
      <xs:documentation>A larger building, not necessarily purpose-built for signalling, that can house cabinets or equipment rooms
&lt;ul&gt;
 &lt;li&gt;NR: a signal box maps to a building.&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;de-DE: Geb&amp;#228;ude&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Housing"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="turnoutComponent" type="signalling:TurnoutComponent"/>
  <xs:complexType name="TurnoutComponent">
    <xs:annotation>
      <xs:documentation>Smallest movable component of a point, a crossing or a derailer.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:MovableComponent"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="maxRouteBodySpeed" type="signalling:MaxRouteBodySpeed"/>
  <xs:complexType name="MaxRouteBodySpeed">
    <xs:annotation>
      <xs:documentation>Maximum (signalled) line speed, i.e. the speed allowed when the signals show 'proceed without restrictions'.
de-DE: Geschwindigkeit, die bei Zugstra&amp;#223;en mit diesem befahrenen Teil ohne Einschr&amp;#228;nkung durch Zs 3 gefahren w&amp;#252;rde (Geschwindigkeit der Strecke).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToRouteBody" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:RouteBody</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasMaxSpeed" type="rtmCommon:Speed"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="localDatum" type="signalling:LocalDatum"/>
  <xs:complexType name="LocalDatum">
    <xs:annotation>
      <xs:documentation>To support track workers in locating the asset, the local datum combines a reference point easily identified in the field (for instance a milepost or overhead catenary mast), and a distance and direction to the asset.
Not for computing the position of the asset. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:attribute name="direction" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Either &amp;lt;plus&amp;gt; or &amp;lt;minus&amp;gt;, incrementing or decrementing kilometrage.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="longitudinalMeasure" type="xs:float" use="required">
          <xs:annotation>
            <xs:documentation>Distance from the datum (positive number).</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="referencePoint" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Name of a well-identifiable object acting as a reference for locating an other track asset.
For instance: Milepost 23 1/2</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="stopMessage" type="signalling:StopMessage"/>
  <xs:complexType name="StopMessage">
    <xs:annotation>
      <xs:documentation>Indicates that the train must stop. 

</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:AspectComposition">
        <xs:attribute name="imperative" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>This boolean modifies of the message.
&lt;ul&gt;
 &lt;li&gt;True: the signalling system ascertains danger beyond the signal: the stop must be respected.&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;False: a failure prevents the signalling system from asserting danger beyond the signal. Typically, the driver may then proceed with caution and permission given by the signalman.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableJoint" type="signalling:CableJoint"/>
  <xs:complexType name="CableJoint">
    <xs:annotation>
      <xs:documentation>A cable joint is where cable terminations are distributed, joined, spliced.
As opposed to the cable joint installation, the cable joint is not housed. The linear location of the cables provide the position inside the cable route.
</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="1" name="realisesCableJoints" type="signalling:CableJointRelation"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="temporaryCableRoute" type="signalling:TemporaryCableRoute"/>
  <xs:complexType name="TemporaryCableRoute">
    <xs:complexContent>
      <xs:extension base="signalling:CableRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="referenceDrawingParameter" type="signalling:ReferenceDrawingParameter"/>
  <xs:complexType name="ReferenceDrawingParameter">
    <xs:annotation>
      <xs:documentation>Some reference drawings contain parameterised sketches that need a specific value in a concrete engineering planning. This concrete value is stored as a key-value pair where the key is the name of the parameter in the drawing and the value is the applicable value.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:attribute name="name" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Name of the parameter in the reference drawing.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="value" type="xs:float" use="required">
          <xs:annotation>
            <xs:documentation>Concrete value for the given parameter.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lozSignalRelation" type="signalling:LozSignalRelation"/>
  <xs:complexType name="LozSignalRelation">
    <xs:annotation>
      <xs:documentation>A signal within the local operating zone. 
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:LozAssetRelation">
        <xs:sequence>
          <xs:element name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ActiveSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="needsAspectForReturn" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The signal must show the given aspect, generally stop, before the local operating zone can be handed back to the signaller.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Aspect</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="assetGroup" type="signalling:AssetGroup"/>
  <xs:complexType abstract="true" name="AssetGroup">
    <xs:annotation>
      <xs:documentation>Assets may need grouping. Typically, a group will be used to hide the underlying members. For instance, a signalman may dim a group of signals.
Given the wide variety of uses, this element is abstract and the end-user, i.e. the IM must specialise this element.
Examples:
&lt;ul&gt;
 &lt;li&gt;Points grouping for purposes of point heating&lt;/li&gt;
 &lt;li&gt;Cascaded points&lt;/li&gt;
 &lt;li&gt;Driver information. de-DE: Weichenbereich. nl-NL: wisselstraat. fr-FR: Reprise info&lt;/li&gt;
 &lt;li&gt;Power supply&lt;/li&gt;
 &lt;li&gt;Day-night signal intensity (de-DE: Nachtschaltung)&lt;/li&gt;
&lt;/ul&gt;

</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="containsTrackAsset" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TrackAsset</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="physicalMDM" type="signalling:PhysicalMDM"/>
  <xs:complexType name="PhysicalMDM">
    <xs:annotation>
      <xs:documentation>A physical MDM device that hosts any number of diagnostics collectors.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:MaintenanceDataManagement">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="isHousedIn" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="physicalSignal" type="signalling:PhysicalSignal"/>
  <xs:complexType name="PhysicalSignal">
    <xs:annotation>
      <xs:documentation>A signal with a physical line-side appearance.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Signal"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lozKeyReleaseInstrumentRelation" type="signalling:LozKeyReleaseInstrumentRelation"/>
  <xs:complexType name="LozKeyReleaseInstrumentRelation">
    <xs:annotation>
      <xs:documentation>A key release instrument within the local operating zone.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:LozAssetRelation">
        <xs:sequence>
          <xs:element name="appliesToKeyReleaseInstrument" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:KeyReleaseInstrument</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="needsKeyPresenceForReturn" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True: The key must be locked inside the key release instrument before the local operating zone can be handed back to the signaller.
nl-NL: grendel moet ter plekke zijn vrijgegeven.

False: the interlocking doesn't check the presence of the key before taking back the zone.

</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="controller" type="signalling:Controller"/>
  <xs:complexType name="Controller">
    <xs:annotation>
      <xs:documentation>Term for constructing the hierarchy of types of objects that control the field elements. 
This controller class can be instantiated when no other appropriate sub-class is available, e.g. when a new kind of controller is introduced.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAsset">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasBackupPowerSupplyType" type="signalling:PowerSupplyTypes"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasPowerSupplyType" type="signalling:PowerSupplyTypes"/>
          <xs:element maxOccurs="1" minOccurs="0" name="isHousedIn" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Housing</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="operatesLocality" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Informs about the locality under control.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:OperationalLocality</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="reportsToDiagnosticsCollector" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:DiagnosticsCollector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="technology" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>The technology of the controller, e.g. digital, electronic, cloud, geographical, tabular, lever, relay, mechanical, pushbutton.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="LevelCrossingActivationTypes">
    <xs:annotation>
      <xs:documentation>The activation depends on the type of level crossing. Some need earlier activation than others. 

Enum: B&amp;#220; Funktions&amp;#252;berwachung</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="locallyMonitored">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="locallyMonitoredOptimisedActivation">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="mainSignalProtected">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="selfSupervised">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="nonStopMovement" type="signalling:NonStopMovement"/>
  <xs:complexType name="NonStopMovement">
    <xs:annotation>
      <xs:documentation>Indicates whether the train may proceed beyond the signal with or without stopping. 
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasCondition" type="signalling:NonStopMovementAllowedTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="BlockCircuitryTypes">
    <xs:annotation>
      <xs:documentation>The number of wires that carry the block information. This characterises the block.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="_3wire">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="_4wire">
        <xs:annotation>
          <xs:documentation>nl-NL: 4-draads APB</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="_6wire">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="_8wire">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="_9wire">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="tdsSection" type="signalling:TdsSection"/>
  <xs:complexType name="TdsSection">
    <xs:annotation>
      <xs:documentation>Train Detection System associated with a TVP section.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAsset">
        <xs:sequence>
          <xs:element name="appliesToTvpSection" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TvpSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="compoundLogicalObject" type="signalling:CompoundLogicalObject"/>
  <xs:complexType name="CompoundLogicalObject">
    <xs:annotation>
      <xs:documentation>The compound logical object is a fa&amp;#231;ade to a system or device implementing Boolean logic. It can represent anything from a simple relay contact to complex relay circuitry or a PLC that converts high/low electric input signals from any source into Boolean true/false outputs. 
It can exchange binary i/o with the interlocking. 
The description attribute can contain textual description of the field elements and Boolean relations that produce the Boolean output.
Use this for ancillary equipment connected to the interlocking, e.g. bascule bridges, tunnel equipment, detectors such as earthquake and flooding detectors. Also useful for reading inputs from field activated level crossings. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAsset">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="isImplementedByDetector" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The logic is implemented by a (set of) line side spot detectors. 
The description of the compound logical object can state the logical equations.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LineSideSpotDetector</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Description" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>Description of the logic.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="standstillDetectionForShunting" type="signalling:StandstillDetectionForShunting"/>
  <xs:complexType name="StandstillDetectionForShunting">
    <xs:annotation>
      <xs:documentation>A timer for measuring the time a train has been stationary before entering the temporary shunting area.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="appliestoTempShuntingArea" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:TempShuntingArea</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="hasStandstillTimer" type="rtmCommon:Duration"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="designation" type="signalling:Designation"/>
  <xs:complexType abstract="true" name="Designation">
    <xs:annotation>
      <xs:documentation>Abstract class for defining labels or complex names to designate a track asset. Alternatively, use the SimpleDesignation class.

Note: Labels and national labelling rules should be defined in the national namespace. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfigurationProperty"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="platformAccess" type="signalling:PlatformAccess"/>
  <xs:complexType name="PlatformAccess">
    <xs:annotation>
      <xs:documentation>Means of platform access. Relevant for positioning operational stops.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="isOfPlatformAccessType" type="signalling:PlatformAccessTypes"/>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="leadsPassengersToPlatformEdgeAccessPoint" type="signalling:PlatformEdgeAccessPoint"/>
        </xs:sequence>
        <xs:attribute name="isPrimaryAccess" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>When true, this is the primary platform access. Otherwise, secondary access.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="attachment" type="signalling:Attachment"/>
  <xs:complexType name="Attachment">
    <xs:annotation>
      <xs:documentation>Additional source of information. 
For instance:  approval document, drawing, sketches, calculations, d&amp;#233;rogation (Fr). 
This should be enriched at the national level with types according to document types such as as-built validation document, etc.

Do not use as a convenience class for storing information covered by existing attributes. 
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="content" type="xs:string">
            <xs:annotation>
              <xs:documentation>The document given as a blob of bytes or an Uniform Resource Indicator (uri), reference to a document in a data base.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="contentType" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>The MIME type of the content.
See: http://www.iana.org/assignments/media-types/media-types.xhtml</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="fileName" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>The name of the file. 
The filename may include the extension and relative path.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tcSensorCombined" type="signalling:TcSensorCombined"/>
  <xs:complexType name="TcSensorCombined">
    <xs:annotation>
      <xs:documentation>Track circuit that is both used for vacancy detection and train approach detection.
de-DE: isolierte Schiene, FMA mitbenutzt.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TcSensor"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="trafficControlSystem" type="signalling:TrafficControlSystem"/>
  <xs:complexType name="TrafficControlSystem">
    <xs:annotation>
      <xs:documentation>A Traffic Control System is located in the
&lt;ul&gt;
 &lt;li&gt;fr-FR: Poste de commande &amp;#224; distance&lt;/li&gt;
 &lt;li&gt;Betriebszentrale in DACH&lt;/li&gt;
 &lt;li&gt;en-UK: signalbox&lt;/li&gt;
 &lt;li&gt;nl-NL: VL-Post&lt;/li&gt;
 &lt;li&gt;it-IT: Posto centrale/locale&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Controller"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="rbcProperties" type="signalling:RbcProperties"/>
  <xs:complexType name="RbcProperties">
    <xs:annotation>
      <xs:documentation>Set of properties that apply to the RBC.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="appliesToRbc" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The properties apply to a given RBC.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:RBC</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasAckTimer" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>RBC acknowledgement timer: A configurable timer used to resend application messages in case of communication loss. The timer starts at the moment of sending the application message for which acknowledgement is required.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasGeneralMessageTimer" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Time interval between general messages from RBC to train. This checks that the train is still communicating with the RBC.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasPermShuntTimer" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Permission to shunt timer. When the driver hasn't started the shunting process before, the permission to shunt is revoked.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="minUserDataRate" type="xs:int">
        <xs:annotation>
          <xs:documentation>The minimal data rate of the train-RBC communication channel in Baud. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="nid_c" type="xs:int">
        <xs:annotation>
          <xs:documentation>NID_C</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="nid_radio" type="xs:string">
        <xs:annotation>
          <xs:documentation>Radio Subscriber Number. This is the number that the ETCS onboards call.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="srsSubversion" type="xs:string">
        <xs:annotation>
          <xs:documentation>The subversion of subset-026-7.
E.g. Maintenance Release 1.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="srsVersion" type="xs:string">
        <xs:annotation>
          <xs:documentation>The version of Subset-026-7.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="stopInShuntingSuppTimer" type="rtmCommon:Duration">
        <xs:annotation>
          <xs:documentation>Stop if in shunting suppression timer:
as long as the timer is running, the train can ignore  "stop if in shunting" message. 

This supposes that a CR has been implemented that the RBC and train remain connected during shunting.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="nid_rbc" type="xs:int" use="required"/>
  </xs:complexType>
  <xs:element name="signal" type="signalling:Signal"/>
  <xs:complexType name="Signal">
    <xs:annotation>
      <xs:documentation>A Signal is a line side element that sends a message to driver. The signal can consist of several frames each of which carries part of the message to the driver.
The signal in the EULYNX DP namespace enriches the RTM information with signalling information.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAsset">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLocalDatum" type="signalling:LocalDatum"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLocation" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SignalLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasSignalFrame" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A signal has one or more signal frames. A signal frame has an aspect that is either passive or active, i.e. that can change aspect. 
A non-physical signal has no frame</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SignalFrame</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isOfFixingType" type="signalling:FixingTypes">
            <xs:annotation>
              <xs:documentation>General information about the fixing of a signal. Mainly used in early design phase and cost calculations.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="refersToOverlapsForDisplay" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The list of overlaps is ordered so the signaller's display shows the overlaps in the given sequence.
de-DE: D-Wege die an einem Signal beginnen.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Overlap</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="refersToRtmSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A EULYNX DP signal refers to a signal in the RTM namespace.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmSig:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="refersToSafetyDistance" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>An exit signal has a list of safety distances, i.e. overlap or danger point.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:SafetyDistance</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="showsDefaultMessage" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The default message that the signal shows after startup and at rest (most restrictive message/aspect).</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:AspectComposition</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="EtcsLevelTypes">
    <xs:annotation>
      <xs:documentation>ETCS level types. 
Engineering rules may depend on the presence of line-side light signals. </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="level0">
        <xs:annotation>
          <xs:documentation>ETCS Level 0 
E.g. shunting area on HSL Zuid (NL)</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="level1">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="level1Ls">
        <xs:annotation>
          <xs:documentation>ETCS Level 1 Limited Supervision.
de-DE: ESG - ETCS Signalgef&amp;#252;hrt.
</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="level2WithoutLightSignals">
        <xs:annotation>
          <xs:documentation>Level 2 without lateral light signalling.
This includes areas that have light signals at the level transitions and/or some yard signals.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="level2WithLightSignals">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="level3">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="level3Hybrid">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="levelNtc">
        <xs:annotation>
          <xs:documentation>This area has a national train control system.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="conditionAffectingRoute" type="signalling:ConditionAffectingRoute"/>
  <xs:complexType name="ConditionAffectingRoute">
    <xs:annotation>
      <xs:documentation>Condition required for the route to open, read from a compound logical object or a release instrument.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:RouteCondition">
        <xs:sequence>
          <xs:element name="affectsRoute" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Route that is allowed if the conditions in the route relation evaluate to true.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Route</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="lxDepartureDetection" type="signalling:LxDepartureDetection"/>
  <xs:complexType name="LxDepartureDetection">
    <xs:annotation>
      <xs:documentation>Parameters for activating the level crossing by detection of departure of an ETCS train. 
nl-NL: vertrekdetectiepunt</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="hasDepartureDetectionPoint" type="rtmCommon:SpotLocation">
            <xs:annotation>
              <xs:documentation>The spot up to where ETCS train departure detection can activate the level crossing.
nl-NL: vertrekdetectiepunt</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasOperationalDepartureArea" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The operational point from which the train leaves (platform track; used for calibrating train runs to the time table).
nl-NL: verwijzing naar het dienstregelpunt (perronspoor) waar vanuit de trein vertrekt.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:PlatformEdge</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasSpeedRestrictionLength" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>The distance over which the restricted speed applies. This allows the LX sufficient time to close.
nl-NL: de lengte van de conditionele snelheidsbeperking naar de overweg van het vertrekdetectiepunt.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="powerSourceRelation" type="signalling:PowerSourceRelation"/>
  <xs:complexType name="PowerSourceRelation">
    <xs:annotation>
      <xs:documentation>Describes the sourcing of power to a power supply. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ProvidesPowerTo" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:PowerSupplyModule</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="getsPowerFromPsModule" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>Gets power from a physical power supply module.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:PhysicalPowerSupplyModule</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="getsPowerFromTrackAsset" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The virtual power supply model gets power from a track asset such as a controller, LX, or TPS device.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:TrackAsset</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="hasPowerSourceType" type="signalling:PowerSourceTypes">
        <xs:annotation>
          <xs:documentation>The power source type is indicated if no actual object that is the source of power can be indicated.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="isPrimarySource" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>True if the source of the power is the primary power.
False otherwise.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="1" name="isUps" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>True if the source of power is buffered (UPS - Uninterruptable Power Supply)
de-DE: USV Unterbrechungsfreie Stromversorgung.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="sidingSection" type="signalling:SidingSection"/>
  <xs:complexType name="SidingSection">
    <xs:annotation>
      <xs:documentation>Track not considered part of the main infrastructure
de-DE: Anschlussgleis, Werksgleis
en-GB: used for shunting
nl-NL: raccordement, opstelspoor
fr-FR: voie de service</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackTypeSection"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableTrench" type="signalling:CableTrench"/>
  <xs:complexType name="CableTrench">
    <xs:annotation>
      <xs:documentation>A trench in the earth where the cable is buried.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="routeTractionType" type="signalling:RouteTractionType"/>
  <xs:complexType name="RouteTractionType">
    <xs:annotation>
      <xs:documentation>The route is suitable for the given type of traction.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:RouteProperty">
        <xs:sequence>
          <xs:element name="isSuitableForTractionType" type="signalling:RouteTractionTypes">
            <xs:annotation>
              <xs:documentation>This route is suitable for the given type of traction.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="conditionAffectingKRI" type="signalling:ConditionAffectingKRI"/>
  <xs:complexType name="ConditionAffectingKRI">
    <xs:annotation>
      <xs:documentation>The condition for releasing the key release instrument, pertaining to the occupancy of the target track of a shunting route.
de-DE: Kriterium f&amp;#252;r die Aufl&amp;#246;sung von &amp;#252;berwachten Schl&amp;#252;sselsperren im Zielgleis von Rangierstra&amp;#223;en.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConditionAffectingRoute">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="needsTargetTrackVacancyState" type="signalling:TargetTrackConditionTypes">
            <xs:annotation>
              <xs:documentation>The required occupancy status of the target track.
de-DE: Kriterium f&amp;#252;r die Aufl&amp;#246;sung von &amp;#252;berwachten Schl&amp;#252;sselsperren im Zielgleis von Rangierstra&amp;#223;en.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="dispatchAnnouncerAssignment" type="signalling:DispatchAnnouncerAssignment"/>
  <xs:complexType name="DispatchAnnouncerAssignment">
    <xs:annotation>
      <xs:documentation>The entry signal is associated either with
&lt;ul&gt;
 &lt;li&gt;a light signal informing train staff to prepare for imminent departure (de-DE: Fahrtank&amp;#252;nder - wei&amp;#223;es Dreieck)&lt;/li&gt;
 &lt;li&gt;a counter indicating the remaining time to planned departure (ProRail)&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;a system that messages the train staff mobile device (de-DE: dann entf&amp;#228;llt der physische Fahrtank&amp;#252;nder).&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="appliesToDispatchAnnouncer" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A special light signal that announces imminent clearing of the entry signal. If not provided, the announcement can be messaged to the train staff's mobile device. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LightSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesToEntrySignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The signal(s) at the entry of routes that the train is due to use.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsMaSection" type="signalling:EtcsMaSection"/>
  <xs:complexType name="EtcsMaSection">
    <xs:annotation>
      <xs:documentation>The Movement Authority (MA) section has an entry location and optionally a timer start and/or stop location.
ETCS variable D_SECTIONTIMERSTOPLOC is the distance between start and stop location.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="hasSectionEntryLocation" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Entry to the MA section.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTimerStartLocation" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The designer marks the spot location where the section timer starts counting down. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTimerStopLocation" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The spot location where the section timer is stopped (&amp;#167;3.8.4.2.3)</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="t_sectiontimer" type="xs:int" use="required">
          <xs:annotation>
            <xs:documentation>maps to T_SECTIONTIMER.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsSpeedChangeOtherCondition" type="signalling:EtcsSpeedChangeOtherCondition"/>
  <xs:complexType name="EtcsSpeedChangeOtherCondition">
    <xs:complexContent>
      <xs:extension base="signalling:EtcsSpeedChangeCondition"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="movableElementAsBuiltConfiguration" type="signalling:MovableElementAsBuiltConfiguration"/>
  <xs:complexType name="MovableElementAsBuiltConfiguration">
    <xs:annotation>
      <xs:documentation>A set of as built properties that applies to a movable element.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackAssetConfiguration">
        <xs:sequence>
          <xs:element name="appliesToMovableElement" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="ExtensionDirectionTypes">
    <xs:annotation>
      <xs:documentation>The direction in which the system extends, given with respect to the linear element.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="nominal">
        <xs:annotation>
          <xs:documentation>Same direction as linear element.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="opposite">
        <xs:annotation>
          <xs:documentation>Opposite direction to linear element.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="none">
        <xs:annotation>
          <xs:documentation>Spot without length (e.g. single balise).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="unknown">
        <xs:annotation>
          <xs:documentation>The extension direction is (yet) unknown.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="maintenanceDataManagement" type="signalling:MaintenanceDataManagement"/>
  <xs:complexType name="MaintenanceDataManagement">
    <xs:annotation>
      <xs:documentation>The Subsystem - Maintenance and Data Management (MDM) runs the services required for the operation of the EULYNX System. Service functions may be provided also to the adjacent systems.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasStartUpConfiguration" type="signalling:StartUpProcedureConfiguration"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hostsDiagnosticsCollector" type="signalling:DiagnosticsCollector">
            <xs:annotation>
              <xs:documentation>The MDM hosts a diagnostic collector service.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="ConductorVoltageTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="1200VDC">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="750VDC">
        <xs:annotation>
          <xs:documentation>750 VDC third rail</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="activeAspectSet" type="signalling:ActiveAspectSet"/>
  <xs:complexType name="ActiveAspectSet">
    <xs:annotation>
      <xs:documentation>A set of actively controlled signal aspects.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="emitsActiveAspect" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Commands emission of this aspect.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:ActiveAspect</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="semantic" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Description of the meaning of the message as a string.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="crossing" type="signalling:Crossing"/>
  <xs:complexType name="Crossing">
    <xs:annotation>
      <xs:documentation>A crossing installation.
Some crossings, especially high speed ones, have a switched diamond to close the gap at the crossing (DE: Flachkreuzung mit beweglichen Herzst&amp;#252;ckspitzen, NL: kruising met beweegbaar puntstuk). 
Strictly speaking such a crossing isn't a turnout because the movable frog will not send the train left or right. This said, it does guide the train and its position is essential for avoiding derailment.
The slip points, if any, to which the crossing connects are separate instances of the Point class.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:PointCrossing">
        <xs:sequence>
          <xs:element maxOccurs="2" minOccurs="1" name="hasDiamond" type="signalling:Diamond"/>
          <xs:element name="isOfCrossingType" type="signalling:CrossingTypes">
            <xs:annotation>
              <xs:documentation>Informs the type that this crossing is part of.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="cableBridge" type="signalling:CableBridge"/>
  <xs:complexType name="CableBridge">
    <xs:annotation>
      <xs:documentation>Structure for guiding cables over track or other obstacle. Can be integral part of an existing bridge or viaduct.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:CableRoute"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="rbcInterlockingCommGroup" type="signalling:RbcInterlockingCommGroup"/>
  <xs:complexType name="RbcInterlockingCommGroup">
    <xs:annotation>
      <xs:documentation>A group of one RBC and one or more interlocking that need to exchange signalling information.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:RbcInterlockingGroup"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="foulTrackControl" type="signalling:FoulTrackControl"/>
  <xs:complexType name="FoulTrackControl">
    <xs:annotation>
      <xs:documentation>The train detector within a fouling track dictates which movable element positions must be checked and which section must be clear as a consequence (if any).
Typically used in Germany.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="alsoCheckMovableElement" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:MovableElement</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="alsoCheckPointElementInPosition" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="alsoCheckTdsSection" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The section that must be proven as extra verification to avoid fouling.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:TdsSection</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="appliesToPointElementInPosition" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:documentation>The branch of the point that cannot be proven vacant without extra verifications.</xs:documentation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:PointElementAndPosition</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="nearestAccessPoint" type="signalling:NearestAccessPoint"/>
  <xs:complexType name="NearestAccessPoint">
    <xs:annotation>
      <xs:documentation>Description of the location where maintenance personnel can gain access to the track asset.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="isRelatedToTrackAsset" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:TrackAsset</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="description" type="xs:string" use="required">
      <xs:annotation>
        <xs:documentation>Textual description.
</xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:element name="etcsArea" type="signalling:EtcsArea"/>
  <xs:complexType name="EtcsArea">
    <xs:annotation>
      <xs:documentation>Area with ETCS supervision.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TpArea">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="allowsTrainsWithEtcsVersion" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The area allows trains equipped with this ETCS system version.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsSystemVersion</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hasEntryLtMarker" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The transition markers are located near, not necessarily exactly on this border because in- and outgoing transitions can differ. Markers help define the functional transitions from one area to another.
Reference to marker where a Level Transition occurs into the area.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsLevelTransitionMarker</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="hasEtcsLevel" type="signalling:EtcsLevelTypes">
            <xs:annotation>
              <xs:documentation>The ETCS level of the area. 
One area per level. Areas can overlap, i.e. a fall-back level 1 would largely overlap the level 2.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isFittedWithEtcsVersion" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The ETCS area is fitted with the given version. May be empty in an early engineering stage. </xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EtcsSystemVersion</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsCesSectionCondition" type="signalling:EtcsCesSectionCondition"/>
  <xs:complexType name="EtcsCesSectionCondition">
    <xs:annotation>
      <xs:documentation>The CES is issued when TVP section(s) is occupied. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EtcsCesCondition">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesWhenSectionIsOccupied" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>When the section is occupied, issue a CES</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TvpSection</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="aspect" type="signalling:Aspect"/>
  <xs:complexType name="Aspect">
    <xs:annotation>
      <xs:documentation>Atomic information displayed by the signal, conveyed by means of light, shape, text, symbol or sound.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="appliesToTrainCategory" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The aspect may apply to a train category. 
If no category is given, the aspect applies to &lt;i&gt;all&lt;/i&gt; categories.
Some IM must explicitly define the train category to which the aspect applies. NR for instance needs a list of train categories to which a speed aspect applies.
See the &lt;u&gt;Speed profiles&lt;/u&gt; package for more information.
</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TrainCategory</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasSymbol" type="signalling:Symbol"/>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTextInformation" type="signalling:TextInformation"/>
        </xs:sequence>
        <xs:attribute name="aspectId" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>An entry in the national signal book or EULYNX regulation, describing the meaning, colour, shape, etc. that define the aspect.
For instance: "Zs 3"</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="indicatesRouteDirection" type="xs:boolean" use="optional">
          <xs:annotation>
            <xs:documentation>The aspect has an &lt;i&gt;implicit &lt;/i&gt;direction indication. 
E.g. a german driver can derive from Zs3 speed indication the direction of the route on grounds of local knowledge (e.g. high speed to Berlin, low speed to Leipzig). This helps the designer to decide whether an explicit direction indicator is needed.
Not filled when the signal has explicit direction indication (NR case)</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="tpsRelation" type="signalling:TpsRelation"/>
  <xs:complexType name="TpsRelation">
    <xs:annotation>
      <xs:documentation>Informs at what distance the TP device or system is placed on approach to the signal. 
The signal could be a speed restriction board (NR approach).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToBufferStop" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>States that the TP system protects a buffer stop (in use with ProRail and NR).</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:BufferStop</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="appliesToRelatedSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The TPS device can reference repeater signals, stop marker boards, distant signals (DB).</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The signal that dictates the position of the TPS device. This is given with the distance from the signal.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToTpsDataTxSystem" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Applies to a TPS data transmission system.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TpsDataTransmissionSystem</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="appliesToTpsDevice" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TpsDevice</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasDistanceFromTpsToSignal" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>Distance from the TP device or system  to the signal.
</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="electrificationGap" type="signalling:ElectrificationGap"/>
  <xs:complexType name="ElectrificationGap">
    <xs:annotation>
      <xs:documentation>Gap in the traction system where no traction power can ever be drawn.
UK: neutral section in overhead line systems, gap in third rail systems.
DE: Trennstelle
NL: spanningsloze sectie
FR: section de s&amp;#233;paration</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ElectrificationSegment"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="levelCrossingDeactivation" type="signalling:LevelCrossingDeactivation"/>
  <xs:complexType name="LevelCrossingDeactivation">
    <xs:annotation>
      <xs:documentation>Deactivation of the level crossing means that the barriers open and/or the road user gets an all-clear.
This class stores parameters relevant to the de-activation process.
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element name="appliestoDangerAreaTrack" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:DangerAreaTrack</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="usesTriggerAssignment" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Link to a trigger assignment that informs about the deactivation of the level crossing.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TriggerAssignment</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="speedSegment" type="signalling:SpeedSegment"/>
  <xs:complexType name="SpeedSegment">
    <xs:annotation>
      <xs:documentation>A segment of a speed profile for which a constant speed applies. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Segment">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasMaximumSpeed" type="rtmCommon:Speed">
            <xs:annotation>
              <xs:documentation>The maximum speed in km/h that applies to the speed section.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="reasonForSpeed" type="xs:string">
            <xs:annotation>
              <xs:documentation>coded or textual reason for a given speed</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="description" type="signalling:Description"/>
  <xs:complexType name="Description">
    <xs:annotation>
      <xs:documentation>To give a detailed description of the object under configuration.
Not to be confused with annotation.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfigurationProperty">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" name="text" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="blockElement" type="signalling:BlockElement"/>
  <xs:complexType name="BlockElement">
    <xs:annotation>
      <xs:documentation>The extremity of the block system. A track that is equipped with a block system has two matching block elements.
de-DE: Element am Ende einer Blockstrecke/Blockanlage. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasBlockClearanceTrigger" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The equipment that reports passage of a train. This supports the clearance-proving of the block. Used for open lines without further technical equipment. 
de-DE: Schaltmittel zur R&amp;#228;umungspr&amp;#252;fung (E-Feld)</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TriggerAssignment</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLineBlockParameters" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The properties of the line, viewed from the block element.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:LineBlockInformation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasTrainRearConfirmation" type="signalling:TrainRearConfirmation">
            <xs:annotation>
              <xs:documentation>A switch or button that is activated to confirm passage of the the rear of the train. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="refersToBlockEntrySignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to the fictitious signal where the train leaves the station and enters the block. 
de-DE: Ausfahrziel Strecke, A-Feld</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:NonPhysicalSignal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="type" type="xs:string">
            <xs:annotation>
              <xs:documentation>The construction type of technology used for the block. Typically relays, auto-block, block instrument, key or token block.
de-DE: Bauform</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="sleeperTrack" type="signalling:SleeperTrack"/>
  <xs:complexType name="SleeperTrack">
    <xs:annotation>
      <xs:documentation>Track with sleepers resting on ballast.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Trackway">
        <xs:sequence>
          <xs:element name="hasSleepersMadeOf" type="signalling:MaterialTypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="mechanism" type="signalling:Mechanism"/>
  <xs:complexType name="Mechanism">
    <xs:annotation>
      <xs:documentation>Mechanism that drives turnout components:
&lt;ul&gt;
 &lt;li&gt;point machine (or drive)&lt;/li&gt;
 &lt;li&gt;hydraulic drive&lt;/li&gt;
 &lt;li&gt;manual levers for hand points&lt;/li&gt;
 &lt;li&gt;wires&lt;/li&gt;
 &lt;li&gt;rods&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:MovableComponent">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="drivesTurnoutComponent" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A mechanism drives any number of turnout components.
E.g. an electric point machine drives two pairs of blades in a double slip crossing</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:TurnoutComponent</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="PowerSupplyModuleType">
    <xs:annotation>
      <xs:documentation>de-DE: EV Modulart</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="autonomous">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="autonomousSmall">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="gridConnected">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="pointOfPower">
        <xs:annotation>
          <xs:documentation>de-DE: PoP-V </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="other">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="EtcsBaselineVersions">
    <xs:annotation>
      <xs:documentation>The ETCS baseline versions.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="MR1">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MR2">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="MR3">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="R1">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="R2">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="R3">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="hviTrackCircuit" type="signalling:HviTrackCircuit"/>
  <xs:complexType name="HviTrackCircuit">
    <xs:annotation>
      <xs:documentation>High Voltage Impulse track circuit.
fr-FR: Impulsion de tension &amp;#233;lev&amp;#233;e (ITE)
nl-NL: prikspanningsspoorstroomloop.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrackCircuit">
        <xs:attribute name="channel" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>Frequency with which the impulses are given.
FR: bande de frequences (F1 .. F4).
</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="signalPostTelephone" type="signalling:SignalPostTelephone"/>
  <xs:complexType name="SignalPostTelephone">
    <xs:annotation>
      <xs:documentation>Telephone for communication between train driver and signalman.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="1" name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="isLocatedAt" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:SpotLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="position" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>Description of the position of the telephone in relation to the associated signal (distance and offset from track)</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="telephoneType" type="xs:string" use="optional">
          <xs:annotation>
            <xs:documentation>Type of telephone: locked, direct line, dial phone. etc.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="locationProxy" type="signalling:LocationProxy"/>
  <xs:complexType abstract="true" name="LocationProxy">
    <xs:annotation>
      <xs:documentation>Facade for a location that may, or not, be associated with an asset.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="freightTrain" type="signalling:FreightTrain"/>
  <xs:complexType name="FreightTrain">
    <xs:annotation>
      <xs:documentation>Category for trains carrying freight or goods.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TrainCategory"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsLevelCondition" type="signalling:EtcsLevelCondition"/>
  <xs:complexType name="EtcsLevelCondition">
    <xs:annotation>
      <xs:documentation>Provides the condition under which a text is displayed or stops being displayed.</xs:documentation>
    </xs:annotation>
    <xs:attribute name="m_leveltextdisplay" type="xs:int" use="required"/>
    <xs:attribute name="nid_ntc" type="xs:int" use="required"/>
  </xs:complexType>
  <xs:element name="signalAlignment" type="signalling:SignalAlignment"/>
  <xs:complexType name="SignalAlignment">
    <xs:annotation>
      <xs:documentation>The signal must be aligned such that the driver can see it. This class describes the position (upstream on the track) of the target of the signal: distance, and optionally height.
This information is typically checked during the installation and testing phases.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="1" minOccurs="0" name="appliesToLightSignal" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:LightSignal</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="appliesToSignalFrame" type="rtmCommon:tElementWithIDref">
        <xs:annotation>
          <xs:appinfo>
            <ObjectTypeRef>signalling:SignalFrame</ObjectTypeRef>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="distance" type="rtmCommon:Length">
        <xs:annotation>
          <xs:documentation>distance along the track to the alignment target </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="1" minOccurs="0" name="height" type="rtmCommon:Length">
        <xs:annotation>
          <xs:documentation>height over the track of the alignment target 
</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="cable" type="signalling:Cable"/>
  <xs:complexType name="Cable">
    <xs:annotation>
      <xs:documentation>Cable, wire or core. 
A cable provides the physical connection.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="enclosesCable" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>A cable can enclose one or more wires. A wire can have one or more cores.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Cable</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasLength" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>In situ required or designed length of cable. 
Can differ from the length of cable route because of the need to cover the last metres to the field element, slack or spare length.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="isLocatedAt" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>The length of the RTM linear location indicates the &lt;i&gt;minimum length &lt;/i&gt;of cable. Additional length has to be added for the distance to the connected elements outside the cable duct topology.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>rtmCommon:LinearLocation</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="constructionArea" type="signalling:ConstructionArea"/>
  <xs:complexType name="ConstructionArea">
    <xs:annotation>
      <xs:documentation>Track area under construction.
de-DE: Baugleis
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:RestrictedArea"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="leuPort" type="signalling:LeuPort"/>
  <xs:complexType name="LeuPort">
    <xs:annotation>
      <xs:documentation>Port between LEU and individually-controlled balise.
The planned and as-built port may differ due to the planning process. Both numbers are stored.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="asBuiltNumber" type="xs:int">
            <xs:annotation>
              <xs:documentation>The as-built port. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="1" name="plannedNumber" type="xs:int">
            <xs:annotation>
              <xs:documentation>Port number as planned during initial engineering.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="localOperationDevice" type="signalling:LocalOperationDevice"/>
  <xs:complexType name="LocalOperationDevice">
    <xs:annotation>
      <xs:documentation>The terminal for locally working line-side elements.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ConfiguredBaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="UiDesignAttachment" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Reference to an attachment that describes the actual user interface of the local operation device in detail. Typically includes a drawing of the layout.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Attachment</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="composedOfUiElement" type="signalling:LocalOperationUiElement">
            <xs:annotation>
              <xs:documentation>The local operation device has a set of buttons, switches.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="hasOperatorPosition" type="signalling:OperatorPosition"/>
          <xs:element name="isOfLocalOperationDeviceType" type="signalling:LocalOperationDeviceTypes"/>
          <xs:element name="isSupportedBy" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>the device is supported by a post or housed in a cabinet.</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:EquipmentSupport</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element maxOccurs="1" minOccurs="0" name="refersToController" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:documentation>Refers to the controller that normally controls the field element (interlocking, decentral interlocking or field object controller).</xs:documentation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Controller</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="etcsCesCondition" type="signalling:EtcsCesCondition"/>
  <xs:complexType name="EtcsCesCondition">
    <xs:annotation>
      <xs:documentation>The condition for issuing a CES.</xs:documentation>
    </xs:annotation>
  </xs:complexType>
  <xs:element name="signalFunction" type="signalling:SignalFunction"/>
  <xs:complexType name="SignalFunction">
    <xs:annotation>
      <xs:documentation>Operational function of the signal, e.g. entry or exit signal, block signal, bridge warning signal. These functions are nationally defined and can be described in the national domains (outside the common EULYNX DP scope).
</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element name="appliesToSignal" type="rtmCommon:tElementWithIDref">
            <xs:annotation>
              <xs:appinfo>
                <ObjectTypeRef>signalling:Signal</ObjectTypeRef>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="value" type="xs:string" use="optional"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="decentralInterlocking" type="signalling:DecentralInterlocking"/>
  <xs:complexType name="DecentralInterlocking">
    <xs:annotation>
      <xs:documentation>Decentral part of the interlocking, running a sub-area of the interlocking area. Typically placed as close as possible to the field elements that it controls.
DB: Gleisfeldkonzentrator (Gfk)</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Interlocking"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="axleCountingSystem" type="signalling:AxleCountingSystem"/>
  <xs:complexType name="AxleCountingSystem">
    <xs:annotation>
      <xs:documentation>An Axle counting system detects trains in a section by counting incoming and outgoing train axles.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TdsSection"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:simpleType name="RouteCategoryTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Main">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="Shunting">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="OnSight">
        <xs:annotation>
          <xs:documentation/>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="trackCircuitProperties" type="signalling:TrackCircuitProperties"/>
  <xs:complexType name="TrackCircuitProperties">
    <xs:complexContent>
      <xs:extension base="signalling:BaseObject">
        <xs:sequence>
          <xs:element maxOccurs="1" minOccurs="0" name="hasElectricLength" type="rtmCommon:Length">
            <xs:annotation>
              <xs:documentation>Electric length of the section, computed according to the regulations and the local particularities.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="ballastResistance" type="xs:int" use="optional">
          <xs:annotation>
            <xs:documentation>Resistance of the track bed of the section (Ohm per km). Given if the train detection technology is a track circuit.
Can be used when calculating the maximum length of the track circuit.
de-DE: Bettungswiderstand</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="localOperationUiIndicator" type="signalling:LocalOperationUiIndicator"/>
  <xs:complexType name="LocalOperationUiIndicator">
    <xs:annotation>
      <xs:documentation>A user interface element; visual or audible indicator, i.e. lamp or buzzer.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:LocalOperationUiElement"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="outOfService" type="signalling:OutOfService"/>
  <xs:complexType name="OutOfService">
    <xs:annotation>
      <xs:documentation>The asset that owns this configuration-status is decommissioned: the element is physically present but disconnected or no longer controlled from the signalling installation (logically disconnected through software).</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:ObjectStatus"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="controlRoom" type="signalling:ControlRoom"/>
  <xs:complexType name="ControlRoom">
    <xs:annotation>
      <xs:documentation>Room in a building housing operational control and command equipment.

DE: Leitstelle
NL: VL-ruimte
FR: Poste de controle</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:EquipmentRoom"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="milepost" type="signalling:Milepost"/>
  <xs:complexType name="Milepost">
    <xs:annotation>
      <xs:documentation>Simple wayside sign marking mileage/kilometrage/hectometre. </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:Sign">
        <xs:attribute name="type" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>Type of the value shown:
&lt;ul&gt;
 &lt;li&gt;UK: quarter-mile, half-mile.&lt;/li&gt;
 &lt;li&gt;Continent:  kilometre, metre, hectometre.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="valueShown" type="xs:string" use="required">
          <xs:annotation>
            <xs:documentation>The mileage or kilometre value written on the milepost</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="facingPointLock" type="signalling:FacingPointLock"/>
  <xs:complexType name="FacingPointLock">
    <xs:annotation>
      <xs:documentation>A movable lock that keeps the point blade(s) in place.
&lt;ul&gt;
 &lt;li&gt;en-GB: Facing Point Lock (FPL). The scheme design provides the label for the designation.&lt;/li&gt;
&lt;/ul&gt;
&lt;ul&gt;
 &lt;li&gt;fr-FR: Verrou Carter Coussinet.&lt;/li&gt;
&lt;/ul&gt;</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="signalling:TurnoutComponent"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="projectPhase" type="signalling:ProjectPhase"/>
  <xs:complexType name="ProjectPhase">
    <xs:annotation>
      <xs:documentation>Indicates the project phase during which the configuration item is defined.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="isOfProjectPhase" type="signalling:ProjectPhaseTypes"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="startUpProcedureConfiguration" type="signalling:StartUpProcedureConfiguration"/>
