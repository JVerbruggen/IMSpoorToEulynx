			<xs:element ref="NID_CTRACTION" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="TextConfirm">
		<xs:sequence>
			<xs:element ref="Q_CONFTEXTDISPLAY"/>
			<xs:element ref="Q_TEXTREPORT"/>
			<xs:group ref="TextReport"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="TextReport">
		<xs:sequence>
			<xs:element ref="NID_TEXTMESSAGE"/>
			<xs:element ref="NID_C"/>
			<xs:element ref="NID_RBC"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="GeographicalPosition">
		<xs:sequence>
			<xs:element ref="Q_NEWCOUNTRY"/>
			<xs:element ref="NID_C" minOccurs="0"/>
			<xs:element ref="NID_BG"/>
			<xs:element ref="D_POSOFF"/>
			<xs:element ref="Q_MPOSITION"/>
			<xs:element ref="M_POSITION"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="ModeProfile">
		<xs:sequence>
			<xs:element ref="D_MAMODE"/>
			<xs:element ref="M_MAMODE"/>
			<xs:element ref="V_MAMODE"/>
			<xs:element ref="L_MAMODE"/>
			<xs:element ref="L_ACKMAMODE"/>
			<xs:element ref="Q_MAMODE"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="UnprotectedLevelCrossing">
		<xs:sequence>
			<xs:element ref="V_LX"/>
			<xs:element ref="Q_STOPLX"/>
			<xs:element ref="L_STOPLX" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<!-- special elements -->
	<xs:element name="OTHER_DATA">
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:any processContents="skip" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
